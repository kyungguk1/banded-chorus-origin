     RowBox[{"exp", "(", 
      RowBox[{"-", 
       FractionBox[
        RowBox[{
         SuperscriptBox["sin", "2"], "\[Alpha]"}], 
        SuperscriptBox["\[Nu]", "2"]]}], ")"}], "sin", "\[VeryThinSpace]", 
     "\[Alpha]"}]}], TraditionalForm]],ExpressionUUID->
  "3cc01ec8-8c1d-4fde-9435-17a1fd138171"],
 ",\nwhere ",
 Cell[BoxData[
  FormBox[
   RowBox[{"0", "\[LessEqual]", "\[Alpha]", "\[LessEqual]", "\[Pi]"}], 
   TraditionalForm]],ExpressionUUID->"d1e34ad0-aa06-4660-925e-59d7b858833c"],
 ".\nSince it is symmetric about ",
 Cell[BoxData[
  FormBox[
   RowBox[{"\[Alpha]", "=", 
    FractionBox["\[Pi]", "2"]}], TraditionalForm]],
  FormatType->TraditionalForm,ExpressionUUID->
  "d91d181e-52d8-41f7-a643-335c2b6a62cb"],
