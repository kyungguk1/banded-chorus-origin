Cell[TextData[{
 "Since we are concerned with the central field line of the mirror magnetic \
field,\nwe can substitute ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    FractionBox[
     RowBox[{"B", "(", "x", ")"}], 
     SubscriptBox["B", "0"]], "=", 
    RowBox[{
     FractionBox[
      RowBox[{
       SubscriptBox["B", "x"], "(", "x", ")"}], 
      SubscriptBox["B", "0"]], "=", 
     RowBox[{
      RowBox[{"1", "+", 
       RowBox[{
        SuperscriptBox["\[Xi]", "2"], 
        SuperscriptBox["x", "2"]}]}], "=", 
      RowBox[{
       SuperscriptBox["sec", "2"], "(", 
       RowBox[{"\[Xi]", " ", 
        SubscriptBox["\[CapitalDelta]", "1"], 
        SuperscriptBox["q", "1"]}], ")"}]}]}]}], TraditionalForm]],
  ExpressionUUID->"0bda8211-9a2c-4bcb-a2a0-32b7b94f1d72"],
 " to the common factor:\n",
 Cell[BoxData[{
  FormBox[
   RowBox[{"\[Eta]", "=", 
    RowBox[{
     FractionBox["n", 
      SubscriptBox["n", "0"]], "=", 
     RowBox[{
      SuperscriptBox[
       RowBox[{"(", 
        RowBox[{
         RowBox[{
          RowBox[{"(", 
           RowBox[{"1", "-", 
            FractionBox[
             SubscriptBox["B", "0"], "B"]}], ")"}], 
          FractionBox[
           SubsuperscriptBox["\[Theta]", 
            RowBox[{"\[UpTee]", 
             RowBox[{",", "0"}]}], "2"], 
           SubsuperscriptBox["\[Theta]", 
            RowBox[{"\[DoubleVerticalBar]", 
             RowBox[{",", "0"}]}], "2"]]}], "+", 
         FractionBox[
          SubscriptBox["B", "0"], "B"]}], ")"}], 
       RowBox[{"-", "1"}]], "=", 
      FractionBox[
       RowBox[{"B", "/", 
        SubscriptBox["B", "0"]}], 
       RowBox[{"1", "-", 
        RowBox[{
         FractionBox[
          SubscriptBox["T", 
           RowBox[{"\[UpTee]", 
            RowBox[{",", "0"}]}]], 
          SubscriptBox["T", 
           RowBox[{"\[DoubleVerticalBar]", 
            RowBox[{",", "0"}]}]]], 
         RowBox[{"(", 
          RowBox[{"1", "-", 
           FractionBox["B", 
            SubscriptBox["B", "0"]]}], ")"}]}]}]]}]}]}], 
   TraditionalForm], "\[IndentingNewLine]", 
  FormBox[
   RowBox[{"=", 
    RowBox[{
     FractionBox[
      RowBox[{"1", "+", 
