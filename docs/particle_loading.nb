      RowBox[{
       SuperscriptBox[
        RowBox[{"(", 
         RowBox[{
          RowBox[{
           RowBox[{"(", 
            RowBox[{"1", "-", 
             FractionBox[
              SubscriptBox["B", "0"], "B"]}], ")"}], 
           FractionBox[
            SubsuperscriptBox["\[Theta]", 
             RowBox[{"\[UpTee]", 
              RowBox[{",", "0"}]}], "2"], 
            SubsuperscriptBox["\[Theta]", 
             RowBox[{"\[DoubleVerticalBar]", 
              RowBox[{",", "0"}]}], "2"]]}], "+", 
