Cell[TextData[{
 "Vector component transformation\n",
 Cell[BoxData[
  FormBox[
   RowBox[{"\[DoubleStruckU]", "=", 
    RowBox[{
     RowBox[{
      SuperscriptBox["u", "i"], 
      SubscriptBox["\[DoubleStruckG]", "i"]}], "=", 
     RowBox[{
      RowBox[{
       SubscriptBox["u", "i"], 
       SuperscriptBox["\[DoubleStruckG]", "i"]}], "=", 
      RowBox[{
       RowBox[{
        SubscriptBox["u", "x"], 
        SubscriptBox["\[DoubleStruckE]", "x"]}], "+", 
       RowBox[{
        SubscriptBox["u", "y"], 
        SubscriptBox["\[DoubleStruckE]", "y"]}]}]}]}]}], TraditionalForm]],
  ExpressionUUID->"156fa580-95d5-46b4-839f-c2d69e2aa97a"],
 "\nContravariant to covariant components:\n",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubscriptBox["u", "i"], "=", 
    RowBox[{
     RowBox[{"\[DoubleStruckU]", "\[CenterDot]", 
      SubscriptBox["\[DoubleStruckG]", "i"]}], "=", 
     RowBox[{
      SubscriptBox["g", 
       RowBox[{"i", "\[VeryThinSpace]", "j"}]], 
      SuperscriptBox["u", "j"]}]}]}], TraditionalForm]],ExpressionUUID->
  "012f05c2-999c-492b-9d07-dac49d06096b"],
 "\nContravariant to cartesian components:\n",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"(", GridBox[{
       {
        SubscriptBox["u", "x"]},
       {
        SubscriptBox["u", "y"]}
      }], ")"}], "=", 
    RowBox[{
     RowBox[{"(", GridBox[{
        {
         RowBox[{"\[DoubleStruckU]", "\[CenterDot]", 
          SubscriptBox["\[DoubleStruckE]", "x"]}]},
        {
         RowBox[{"\[DoubleStruckU]", "\[CenterDot]", 
          SubscriptBox["\[DoubleStruckE]", "y"]}]}
       }], ")"}], "=", 
     RowBox[{"(", GridBox[{
        {
         RowBox[{
          RowBox[{
           SuperscriptBox["u", "1"], 
           RowBox[{
            SubscriptBox["\[DoubleStruckG]", "1"], "\[CenterDot]", 
            SubscriptBox["\[DoubleStruckE]", "x"]}]}], "+", 
          RowBox[{
           SuperscriptBox["u", "2"], 
           RowBox[{
            SubscriptBox["\[DoubleStruckG]", "2"], "\[CenterDot]", 
            SubscriptBox["\[DoubleStruckE]", "x"]}]}]}]},
        {
         RowBox[{
          RowBox[{
           SuperscriptBox["u", "1"], 
           RowBox[{
            SubscriptBox["\[DoubleStruckG]", "1"], "\[CenterDot]", 
            SubscriptBox["\[DoubleStruckE]", "y"]}]}], "+", 
          RowBox[{
           SuperscriptBox["u", "2"], 
           RowBox[{
            SubscriptBox["\[DoubleStruckG]", "2"], "\[CenterDot]", 
            SubscriptBox["\[DoubleStruckE]", "y"]}]}]}]}
       }], ")"}]}]}], TraditionalForm]],ExpressionUUID->
  "befde7c4-abf5-4795-b699-3433550de494"],
 "\nCartesian to contravariant components:\n",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SuperscriptBox["u", "i"], "=", 
    RowBox[{
     RowBox[{"\[DoubleStruckU]", "\[CenterDot]", 
      SuperscriptBox["\[DoubleStruckG]", "i"]}], "=", 
     RowBox[{
      RowBox[{
       SubscriptBox["u", "x"], 
       RowBox[{
        SubscriptBox["\[DoubleStruckE]", "x"], "\[CenterDot]", 
        SuperscriptBox["\[DoubleStruckG]", "i"]}]}], "+", 
      RowBox[{
       SubscriptBox["u", "y"], 
       RowBox[{
        SubscriptBox["\[DoubleStruckE]", "y"], "\[CenterDot]", 
        SuperscriptBox["\[DoubleStruckG]", "i"]}]}]}]}]}], TraditionalForm]],
  ExpressionUUID->"7f21b486-68dc-4937-9e8c-4a77072f1d65"]
}], "Text",ExpressionUUID->"f5e1606a-588b-4cc9-8e92-7886fbe0d601"],

