Cell[TextData[{
 "Covariant bases:\n",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubscriptBox["\[DoubleStruckG]", "1"], "=", 
    RowBox[{
     FractionBox[
      RowBox[{"\[PartialD]", "\[DoubleStruckR]"}], 
      RowBox[{"\[PartialD]", 
       SuperscriptBox["q", "1"]}]], "=", 
     RowBox[{
      RowBox[{
       SubscriptBox["\[CapitalDelta]", "1"], "(", 
       RowBox[{
        RowBox[{
         RowBox[{
          SuperscriptBox["sec", "2"], "[", 
          RowBox[{
           SubscriptBox["\[CapitalDelta]", "1"], 
           SuperscriptBox["q", "1"]}], "]"}], 
         SubscriptBox["\[DoubleStruckE]", "x"]}], "-", 
        RowBox[{
         SubscriptBox["\[CapitalDelta]", "2"], 
         SuperscriptBox["q", "2"], 
         RowBox[{"sin", "[", 
          RowBox[{"2", 
           SubscriptBox["\[CapitalDelta]", "1"], 
           SuperscriptBox["q", "1"]}], "]"}], 
         SubscriptBox["\[DoubleStruckE]", "y"]}]}], ")"}], "=", 
      RowBox[{
       RowBox[{
        SubscriptBox["\[CapitalDelta]", "1"], "(", 
        RowBox[{
         RowBox[{
          RowBox[{"(", 
           RowBox[{"1", "+", 
            SuperscriptBox["x", "2"]}], ")"}], 
          SubscriptBox["\[DoubleStruckE]", "x"]}], "-", 
         RowBox[{"2", "x", " ", "y", " ", 
          SubscriptBox["\[DoubleStruckE]", "y"]}]}], ")"}], "=", 
       RowBox[{
        SubscriptBox["\[CapitalDelta]", "1"], 
        FractionBox["\[DoubleStruckCapitalB]", 
         SubscriptBox["B", "0"]]}]}]}]}]}], TraditionalForm]],ExpressionUUID->
  "4146736c-4f89-45c2-966e-cdd2751265f6"],
