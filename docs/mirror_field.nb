Cell[TextData[{
 "Having established the relationship between the two different coordinate \
sets, let us now find the metric and basis vectors.\nThe covariant basis \
vectors are given by ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubscriptBox["\[DoubleStruckG]", "j"], "=", 
    FractionBox[
     RowBox[{"\[PartialD]", "\[DoubleStruckR]"}], 
     RowBox[{"\[PartialD]", 
      SuperscriptBox["q", "j"]}]]}], TraditionalForm]],
  FormatType->TraditionalForm,ExpressionUUID->
  "5a6b936c-276b-45ce-ae0c-9696503f51fd"],
 ":\n",
 Cell[BoxData[{
  FormBox[
   RowBox[{
    SubscriptBox["\[DoubleStruckG]", "1"], "=", 
    RowBox[{
     FractionBox[
      RowBox[{"\[PartialD]", "\[DoubleStruckR]"}], 
      RowBox[{"\[PartialD]", 
       SuperscriptBox["q", "1"]}]], "=", 
     RowBox[{
      RowBox[{
       SubscriptBox["\[CapitalDelta]", "1"], 
       RowBox[{
        SuperscriptBox["sec", "2"], "(", 
        RowBox[{"\[Xi]", " ", 
         SubscriptBox["\[CapitalDelta]", "1"], 
         SuperscriptBox["q", "1"]}], ")"}], 
       SubscriptBox["\[DoubleStruckE]", "x"]}], "-", 
      RowBox[{
       SubscriptBox["\[CapitalDelta]", "1"], "\[Xi]", " ", 
       SubscriptBox["\[CapitalDelta]", "2"], 
       SuperscriptBox["q", "2"], 
       RowBox[{"sin", "(", 
        RowBox[{"\[Xi]", " ", 
         SubscriptBox["\[CapitalDelta]", "1"], 
         SuperscriptBox["q", "1"]}], ")"}], 
       SubscriptBox["\[DoubleStruckE]", "y"]}], "-", 
      RowBox[{
       SubscriptBox["\[CapitalDelta]", "1"], "\[Xi]", " ", 
       SubscriptBox["\[CapitalDelta]", "3"], 
       SuperscriptBox["q", "3"], 
       RowBox[{"sin", "(", 
        RowBox[{"\[Xi]", " ", 
         SubscriptBox["\[CapitalDelta]", "1"], 
         SuperscriptBox["q", "1"]}], ")"}], 
       SubscriptBox["\[DoubleStruckE]", "z"]}]}]}]}], 
   TraditionalForm], "\[IndentingNewLine]", 
  FormBox[
   RowBox[{"=", 
    RowBox[{
     SubscriptBox["\[CapitalDelta]", "1"], "(", 
     RowBox[{
      RowBox[{
       RowBox[{
        SuperscriptBox["sec", "2"], "(", 
        RowBox[{"\[Xi]", " ", 
         SubscriptBox["\[CapitalDelta]", "1"], 
         SuperscriptBox["q", "1"]}], ")"}], 
       SubscriptBox["\[DoubleStruckE]", "x"]}], "-", 
      RowBox[{"\[Xi]", " ", 
       SubscriptBox["\[CapitalDelta]", "2"], 
       SuperscriptBox["q", "2"], 
       RowBox[{"sin", "(", 
        RowBox[{"\[Xi]", " ", 
         SubscriptBox["\[CapitalDelta]", "1"], 
         SuperscriptBox["q", "1"]}], ")"}], 
       SubscriptBox["\[DoubleStruckE]", "y"]}], "-", 
      RowBox[{"\[Xi]", " ", 
       SubscriptBox["\[CapitalDelta]", "3"], 
       SuperscriptBox["q", "3"], 
       RowBox[{"sin", "(", 
        RowBox[{"\[Xi]", " ", 
         SubscriptBox["\[CapitalDelta]", "1"], 
         SuperscriptBox["q", "1"]}], ")"}], 
       SubscriptBox["\[DoubleStruckE]", "z"]}]}], ")"}]}], 
   TraditionalForm], "\[IndentingNewLine]", 
  FormBox[
   RowBox[{"=", 
    RowBox[{
     SubscriptBox["\[CapitalDelta]", "1"], "(", 
     RowBox[{
      RowBox[{
       RowBox[{"(", 
        RowBox[{"1", "+", 
         RowBox[{
          SuperscriptBox["\[Xi]", "2"], 
          SuperscriptBox["x", "2"]}]}], ")"}], 
       SubscriptBox["\[DoubleStruckE]", "x"]}], "-", 
      RowBox[{
       SuperscriptBox["\[Xi]", "2"], "x", " ", "y", " ", 
       SubscriptBox["\[DoubleStruckE]", "y"]}], "-", 
      RowBox[{
       SuperscriptBox["\[Xi]", "2"], "x", " ", "z", " ", 
       SubscriptBox["\[DoubleStruckE]", "z"]}]}], ")"}]}], 
   TraditionalForm], "\[IndentingNewLine]", 
  FormBox[
   RowBox[{"=", 
    RowBox[{
     SubscriptBox["\[CapitalDelta]", "1"], 
     FractionBox["\[DoubleStruckCapitalB]", 
      SubscriptBox["B", "0"]]}]}], TraditionalForm]}],
  FormatType->TraditionalForm,ExpressionUUID->
  "f4d2c731-8820-4dea-a57d-6c0b7a1a615b"],
 ";\n",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubscriptBox["\[DoubleStruckG]", "2"], "=", 
    RowBox[{
     FractionBox[
      RowBox[{"\[PartialD]", "\[DoubleStruckR]"}], 
      RowBox[{"\[PartialD]", 
       SuperscriptBox["q", "2"]}]], "=", 
     RowBox[{
      RowBox[{
       SubscriptBox["\[CapitalDelta]", "2"], 
       RowBox[{"cos", "(", 
        RowBox[{"\[Xi]", " ", 
         SubscriptBox["\[CapitalDelta]", "1"], 
         SuperscriptBox["q", "1"]}], ")"}], 
       SubscriptBox["\[DoubleStruckE]", "y"]}], "=", 
      RowBox[{
       FractionBox[
        SubscriptBox["\[CapitalDelta]", "2"], 
        SqrtBox[
         RowBox[{"1", "+", 
          RowBox[{
           SuperscriptBox["\[Xi]", "2"], 
           SuperscriptBox["x", "2"]}]}]]], "=", 
       RowBox[{
        SubscriptBox["\[CapitalDelta]", "2"], 
        SqrtBox[
         FractionBox[
          SubscriptBox["B", "0"], 
          SubscriptBox["B", "x"]]], 
        SubscriptBox["\[DoubleStruckE]", "y"]}]}]}]}]}], TraditionalForm]],
  FormatType->TraditionalForm,ExpressionUUID->
  "c8fde728-c51c-4edb-94c3-c057229af773"],
 ";\n",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubscriptBox["\[DoubleStruckG]", "3"], "=", 
    RowBox[{
     FractionBox[
      RowBox[{"\[PartialD]", "\[DoubleStruckR]"}], 
      RowBox[{"\[PartialD]", 
       SuperscriptBox["q", "3"]}]], "=", 
     RowBox[{
      RowBox[{
       SubscriptBox["\[CapitalDelta]", "3"], 
       RowBox[{"cos", "(", 
        RowBox[{"\[Xi]", " ", 
         SubscriptBox["\[CapitalDelta]", "1"], 
         SuperscriptBox["q", "1"]}], ")"}], 
       SubscriptBox["\[DoubleStruckE]", "z"]}], "=", 
      RowBox[{
       FractionBox[
        SubscriptBox["\[CapitalDelta]", "3"], 
        SqrtBox[
         RowBox[{"1", "+", 
          RowBox[{
           SuperscriptBox["\[Xi]", "2"], 
           SuperscriptBox["x", "2"]}]}]]], "=", 
       RowBox[{
        SubscriptBox["\[CapitalDelta]", "3"], 
        SqrtBox[
         FractionBox[
          SubscriptBox["B", "0"], 
          SubscriptBox["B", "x"]]], 
        SubscriptBox["\[DoubleStruckE]", "z"]}]}]}]}]}], TraditionalForm]],
  FormatType->TraditionalForm,ExpressionUUID->
  "613c65ba-8dd1-4c4b-9cc8-73d0310c7d1d"],
 ";\nwhere we have used\n",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     SuperscriptBox["sec", "2"], "(", 
     RowBox[{"\[Xi]", " ", 
      SubscriptBox["\[CapitalDelta]", "1"], 
      SuperscriptBox["q", "1"]}], ")"}], "=", 
    RowBox[{
     RowBox[{"1", "+", 
      RowBox[{
       SuperscriptBox["\[Xi]", "2"], 
       SuperscriptBox["x", "2"]}]}], "=", 
     FractionBox[
