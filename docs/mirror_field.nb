Cell[BoxData[
 RowBox[{"With", "[", 
  RowBox[{
   RowBox[{"{", 
    RowBox[{
     RowBox[{"\[Xi]", "=", "2"}], ",", 
     RowBox[{"z0", "=", "0"}], ",", 
     RowBox[{"y0", "=", 
      RowBox[{"Range", "[", 
       RowBox[{
        RowBox[{"-", "1"}], ",", "1", ",", ".2"}], "]"}]}], ",", 
     RowBox[{"xlim", "=", 
      RowBox[{"{", 
       RowBox[{
        RowBox[{"-", "1"}], ",", "1"}], "}"}]}]}], "}"}], ",", 
   "\[IndentingNewLine]", 
   RowBox[{"Module", "[", 
    RowBox[{
     RowBox[{"{", 
      RowBox[{"xs", ",", "ys"}], "}"}], ",", "\[IndentingNewLine]", 
     RowBox[{
      RowBox[{"xs", "=", 
       RowBox[{"Array", "[", 
        RowBox[{"N", ",", "101", ",", "xlim"}], "]"}]}], ";", 
      "\[IndentingNewLine]", 
