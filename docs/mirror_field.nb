              RowBox[{"2", 
               SuperscriptBox["y", "2"]}]}], ")"}], 
            SuperscriptBox["x", "2"]}]}], ")"}], 
         RowBox[{
          SubscriptBox["\[CapitalDelta]", "1"], 
          SubscriptBox["\[CapitalDelta]", "2"], 
          FractionBox[
           RowBox[{
            RowBox[{"-", "2"}], "x", " ", "y"}], 
           RowBox[{"1", "+", 
            SuperscriptBox["x", "2"]}]]}]},
        {
         RowBox[{
          SubscriptBox["\[CapitalDelta]", "1"], 
          SubscriptBox["\[CapitalDelta]", "2"], 
          FractionBox[
           RowBox[{
            RowBox[{"-", "2"}], "x", " ", "y"}], 
           RowBox[{"1", "+", 
            SuperscriptBox["x", "2"]}]]}], 
         FractionBox[
          SubsuperscriptBox["\[CapitalDelta]", "2", "2"], 
          SuperscriptBox[
           RowBox[{"(", 
            RowBox[{"1", "+", 
             SuperscriptBox["x", "2"]}], ")"}], "2"]]}
       }], ")"}], "=", 
     RowBox[{"(", GridBox[{
        {
         RowBox[{
          SubsuperscriptBox["\[CapitalDelta]", "1", "2"], "(", 
          RowBox[{
           RowBox[{
            SuperscriptBox["sec", "4"], "[", 
            RowBox[{
             SubscriptBox["\[CapitalDelta]", "1"], 
             SuperscriptBox["q", "1"]}], "]"}], "+", 
           RowBox[{
            SuperscriptBox[
             RowBox[{"(", 
              RowBox[{
               SubscriptBox["\[CapitalDelta]", "2"], 
               SuperscriptBox["q", "2"]}], ")"}], "2"], 
            RowBox[{
             SuperscriptBox["sin", "2"], "[", 
             RowBox[{"2", 
              SubscriptBox["\[CapitalDelta]", "1"], 
              SuperscriptBox["q", "1"]}], "]"}]}]}], ")"}], 
         RowBox[{
          SubscriptBox["\[CapitalDelta]", "1"], 
          RowBox[{
           SubscriptBox["\[CapitalDelta]", "2"], "(", 
           RowBox[{
            RowBox[{"-", "2"}], 
