Cell["Vector Transformations", "Subsection",ExpressionUUID->"500e6c2e-26de-424e-9e96-815cc090d9cb"],

Cell[TextData[{
 "Consider a vector \n",
 Cell[BoxData[{
  FormBox[
   RowBox[{"\[DoubleStruckCapitalV]", "=", 
    RowBox[{
     RowBox[{
      RowBox[{
       SubscriptBox["V", "x"], 
       SubscriptBox["\[DoubleStruckE]", "x"]}], "+", 
      RowBox[{
       SubscriptBox["V", "y"], 
       SubscriptBox["\[DoubleStruckE]", "y"]}], "+", 
      RowBox[{
       SubscriptBox["V", "z"], 
       SubscriptBox["\[DoubleStruckE]", "z"]}]}], "=", 
     RowBox[{
      RowBox[{
       SubscriptBox["V", "x"], 
       SubscriptBox["\[DoubleStruckE]", "x"]}], "+", 
      RowBox[{
       SubscriptBox["V", "s"], 
       SubscriptBox["\[DoubleStruckE]", "s"]}], "+", 
      RowBox[{
       SubscriptBox["V", "\[Phi]"], 
       SubscriptBox["\[DoubleStruckE]", "\[Phi]"]}]}]}]}], 
   TraditionalForm], "\[IndentingNewLine]", 
  FormBox[
   RowBox[{"=", 
    RowBox[{
     RowBox[{
      SuperscriptBox["V", "j"], 
      SubscriptBox["\[DoubleStruckG]", "j"]}], "=", 
     RowBox[{
      SubscriptBox["V", "j"], 
      SuperscriptBox["\[DoubleStruckG]", "j"]}]}]}], TraditionalForm]}],
  FormatType->TraditionalForm,ExpressionUUID->
  "44330741-d757-47f1-ac8a-bdfa4b140610"],
 ".\n- From Cartesian to cylindrical:\n",
 Cell[BoxData[{
  FormBox[
   RowBox[{
    SubscriptBox["V", "x"], "=", 
    RowBox[{
     RowBox[{"\[DoubleStruckCapitalV]", "\[CenterDot]", 
      SubscriptBox["\[DoubleStruckE]", "x"]}], "=", 
     RowBox[{
      RowBox[{
       RowBox[{"(", 
        RowBox[{
         RowBox[{
          SubscriptBox["V", "x"], 
          SubscriptBox["\[DoubleStruckE]", "x"]}], "+", 
         RowBox[{
          SubscriptBox["V", "y"], 
          SubscriptBox["\[DoubleStruckE]", "y"]}], "+", 
         RowBox[{
          SubscriptBox["V", "z"], 
          SubscriptBox["\[DoubleStruckE]", "z"]}]}], ")"}], "\[CenterDot]", 
       SubscriptBox["\[DoubleStruckE]", "x"]}], "=", 
      RowBox[{
       RowBox[{
        RowBox[{
         SubscriptBox["\[DoubleStruckE]", "x"], "\[CenterDot]", 
         SubscriptBox["\[DoubleStruckE]", "x"]}], 
        SubscriptBox["V", "x"]}], "+", 
       RowBox[{
        RowBox[{
         SubscriptBox["\[DoubleStruckE]", "y"], "\[CenterDot]", 
         SubscriptBox["\[DoubleStruckE]", "x"]}], 
        SubscriptBox["V", "y"]}], "+", 
       RowBox[{
        RowBox[{
         SubscriptBox["\[DoubleStruckE]", "z"], "\[CenterDot]", 
         SubscriptBox["\[DoubleStruckE]", "x"]}], 
        SubscriptBox["V", "z"]}]}]}]}]}], 
   TraditionalForm], "\[IndentingNewLine]", 
  FormBox[
   RowBox[{
    SubscriptBox["V", "s"], "=", 
    RowBox[{
     RowBox[{"\[DoubleStruckCapitalV]", "\[CenterDot]", 
      SubscriptBox["\[DoubleStruckE]", "s"]}], "=", 
     RowBox[{
      RowBox[{
       RowBox[{"(", 
        RowBox[{
         RowBox[{
          SubscriptBox["V", "x"], 
          SubscriptBox["\[DoubleStruckE]", "x"]}], "+", 
         RowBox[{
          SubscriptBox["V", "y"], 
          SubscriptBox["\[DoubleStruckE]", "y"]}], "+", 
         RowBox[{
          SubscriptBox["V", "z"], 
          SubscriptBox["\[DoubleStruckE]", "z"]}]}], ")"}], "\[CenterDot]", 
       SubscriptBox["\[DoubleStruckE]", "s"]}], "=", 
      RowBox[{
       RowBox[{
        RowBox[{
         SubscriptBox["\[DoubleStruckE]", "x"], "\[CenterDot]", 
         SubscriptBox["\[DoubleStruckE]", "s"]}], 
        SubscriptBox["V", "x"]}], "+", 
       RowBox[{
        RowBox[{
         SubscriptBox["\[DoubleStruckE]", "y"], "\[CenterDot]", 
         SubscriptBox["\[DoubleStruckE]", "s"]}], 
        SubscriptBox["V", "y"]}], "+", 
       RowBox[{
        RowBox[{
         SubscriptBox["\[DoubleStruckE]", "z"], "\[CenterDot]", 
         SubscriptBox["\[DoubleStruckE]", "s"]}], 
        SubscriptBox["V", "z"]}]}]}]}]}], 
   TraditionalForm], "\[IndentingNewLine]", 
  FormBox[
   RowBox[{
    SubscriptBox["V", "\[Phi]"], "=", 
    RowBox[{
     RowBox[{"\[DoubleStruckCapitalV]", "\[CenterDot]", 
      SubscriptBox["\[DoubleStruckE]", "\[Phi]"]}], "=", 
     RowBox[{
      RowBox[{
       RowBox[{"(", 
        RowBox[{
         RowBox[{
          SubscriptBox["V", "x"], 
          SubscriptBox["\[DoubleStruckE]", "x"]}], "+", 
         RowBox[{
          SubscriptBox["V", "y"], 
          SubscriptBox["\[DoubleStruckE]", "y"]}], "+", 
         RowBox[{
          SubscriptBox["V", "z"], 
          SubscriptBox["\[DoubleStruckE]", "z"]}]}], ")"}], "\[CenterDot]", 
       SubscriptBox["\[DoubleStruckE]", "\[Phi]"]}], "=", 
      RowBox[{
       RowBox[{
        RowBox[{
         SubscriptBox["\[DoubleStruckE]", "x"], "\[CenterDot]", 
         SubscriptBox["\[DoubleStruckE]", "\[Phi]"]}], 
        SubscriptBox["V", "x"]}], "+", 
       RowBox[{
        RowBox[{
         SubscriptBox["\[DoubleStruckE]", "y"], "\[CenterDot]", 
         SubscriptBox["\[DoubleStruckE]", "\[Phi]"]}], 
        SubscriptBox["V", "y"]}], "+", 
       RowBox[{
        RowBox[{
         SubscriptBox["\[DoubleStruckE]", "z"], "\[CenterDot]", 
         SubscriptBox["\[DoubleStruckE]", "\[Phi]"]}], 
        SubscriptBox["V", "z"]}]}]}]}]}], TraditionalForm]}],
  FormatType->TraditionalForm,ExpressionUUID->
  "b62c6a2c-8317-428c-a655-c0424ed90d21"],
 ",\nor in matrix form,\n",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"(", GridBox[{
       {
        SubscriptBox["V", "x"]},
       {
        SubscriptBox["V", "s"]},
       {
        SubscriptBox["V", "\[Phi]"]}
      }], ")"}], "=", 
    RowBox[{
     RowBox[{
      RowBox[{"(", GridBox[{
         {
          RowBox[{
           SubscriptBox["\[DoubleStruckE]", "x"], "\[CenterDot]", 
           SubscriptBox["\[DoubleStruckE]", "x"]}], 
          RowBox[{
           SubscriptBox["\[DoubleStruckE]", "y"], "\[CenterDot]", 
           SubscriptBox["\[DoubleStruckE]", "x"]}], 
          RowBox[{
           SubscriptBox["\[DoubleStruckE]", "z"], "\[CenterDot]", 
           SubscriptBox["\[DoubleStruckE]", "x"]}]},
         {
          RowBox[{
           SubscriptBox["\[DoubleStruckE]", "x"], "\[CenterDot]", 
           SubscriptBox["\[DoubleStruckE]", "s"]}], 
          RowBox[{
           SubscriptBox["\[DoubleStruckE]", "y"], "\[CenterDot]", 
           SubscriptBox["\[DoubleStruckE]", "s"]}], 
          RowBox[{
           SubscriptBox["\[DoubleStruckE]", "z"], "\[CenterDot]", 
           SubscriptBox["\[DoubleStruckE]", "s"]}]},
         {
          RowBox[{
           SubscriptBox["\[DoubleStruckE]", "x"], "\[CenterDot]", 
           SubscriptBox["\[DoubleStruckE]", "\[Phi]"]}], 
          RowBox[{
           SubscriptBox["\[DoubleStruckE]", "y"], "\[CenterDot]", 
           SubscriptBox["\[DoubleStruckE]", "\[Phi]"]}], 
          RowBox[{
           SubscriptBox["\[DoubleStruckE]", "z"], "\[CenterDot]", 
           SubscriptBox["\[DoubleStruckE]", "\[Phi]"]}]}
        }], ")"}], 
      RowBox[{"(", GridBox[{
         {
          SubscriptBox["V", "x"]},
         {
          SubscriptBox["V", "y"]},
         {
          SubscriptBox["V", "z"]}
        }], ")"}]}], "=", 
     RowBox[{
      RowBox[{
       RowBox[{"(", GridBox[{
          {"1", "0", "0"},
          {"0", 
           RowBox[{"cos", "\[VeryThinSpace]", "\[Phi]"}], 
           RowBox[{"sin", "\[VeryThinSpace]", "\[Phi]"}]},
          {"0", 
           RowBox[{
            RowBox[{"-", "sin"}], "\[VeryThinSpace]", "\[Phi]"}], 
           RowBox[{"cos", "\[VeryThinSpace]", "\[Phi]"}]}
         }], ")"}], 
       RowBox[{"(", GridBox[{
          {
           SubscriptBox["V", "x"]},
          {
           SubscriptBox["V", "y"]},
          {
           SubscriptBox["V", "z"]}
         }], ")"}]}], "=", 
      RowBox[{
       SubscriptBox["\[GothicCapitalL]", 
        RowBox[{"\[DoubleStruckX]", "\[Rule]", 
         RowBox[{"(", 
          RowBox[{"x", ",", "s", ",", "\[Phi]"}], ")"}]}]], "(", GridBox[{
         {
          SubscriptBox["V", "x"]},
         {
          SubscriptBox["V", "y"]},
         {
          SubscriptBox["V", "z"]}
        }], ")"}]}]}]}], TraditionalForm]],
  FormatType->TraditionalForm,ExpressionUUID->
  "67db5613-1aa1-4bc8-83b3-0ca53bba7008"],
 ".\n- From cylindrical to Cartesian:\n",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"(", GridBox[{
       {
        SubscriptBox["V", "x"]},
       {
        SubscriptBox["V", "y"]},
       {
        SubscriptBox["V", "z"]}
      }], ")"}], "=", 
    RowBox[{
     RowBox[{
      RowBox[{"(", GridBox[{
         {
          RowBox[{
           SubscriptBox["\[DoubleStruckE]", "x"], "\[CenterDot]", 
           SubscriptBox["\[DoubleStruckE]", "x"]}], 
          RowBox[{
           SubscriptBox["\[DoubleStruckE]", "s"], "\[CenterDot]", 
           SubscriptBox["\[DoubleStruckE]", "x"]}], 
          RowBox[{
           SubscriptBox["\[DoubleStruckE]", "\[Phi]"], "\[CenterDot]", 
           SubscriptBox["\[DoubleStruckE]", "x"]}]},
         {
          RowBox[{
           SubscriptBox["\[DoubleStruckE]", "x"], "\[CenterDot]", 
           SubscriptBox["\[DoubleStruckE]", "y"]}], 
          RowBox[{
           SubscriptBox["\[DoubleStruckE]", "s"], "\[CenterDot]", 
           SubscriptBox["\[DoubleStruckE]", "y"]}], 
          RowBox[{
           SubscriptBox["\[DoubleStruckE]", "\[Phi]"], "\[CenterDot]", 
           SubscriptBox["\[DoubleStruckE]", "y"]}]},
         {
          RowBox[{
           SubscriptBox["\[DoubleStruckE]", "x"], "\[CenterDot]", 
           SubscriptBox["\[DoubleStruckE]", "z"]}], 
          RowBox[{
           SubscriptBox["\[DoubleStruckE]", "s"], "\[CenterDot]", 
           SubscriptBox["\[DoubleStruckE]", "z"]}], 
          RowBox[{
           SubscriptBox["\[DoubleStruckE]", "\[Phi]"], "\[CenterDot]", 
           SubscriptBox["\[DoubleStruckE]", "z"]}]}
        }], ")"}], 
      RowBox[{"(", GridBox[{
         {
          SubscriptBox["V", "x"]},
         {
          SubscriptBox["V", "s"]},
         {
          SubscriptBox["V", "\[Phi]"]}
        }], ")"}]}], "=", 
     RowBox[{
      RowBox[{
       SubscriptBox["\[GothicCapitalL]", 
        RowBox[{
         RowBox[{"(", 
          RowBox[{"x", ",", "s", ",", "\[Phi]"}], ")"}], "\[Rule]", 
         "\[DoubleStruckX]"}]], "(", GridBox[{
         {
          SubscriptBox["V", "x"]},
         {
          SubscriptBox["V", "s"]},
         {
          SubscriptBox["V", "\[Phi]"]}
        }], ")"}], "=", 
      RowBox[{
       RowBox[{
        RowBox[{
         SubscriptBox["\[GothicCapitalL]", 
          RowBox[{"\[DoubleStruckX]", "\[Rule]", 
           RowBox[{"(", 
            RowBox[{"x", ",", "s", ",", "\[Phi]"}], ")"}]}]], 
         "\[Transpose]"}], 
        RowBox[{"(", GridBox[{
           {
            SubscriptBox["V", "x"]},
           {
            SubscriptBox["V", "s"]},
           {
            SubscriptBox["V", "\[Phi]"]}
          }], ")"}]}], "=", 
       RowBox[{
        RowBox[{"(", GridBox[{
           {
            SubscriptBox["V", "x"], 
            SubscriptBox["V", "s"], 
            SubscriptBox["V", "\[Phi]"]}
          }], ")"}], 
        SubscriptBox["\[GothicCapitalL]", 
         RowBox[{"\[DoubleStruckX]", "\[Rule]", 
          RowBox[{"(", 
           RowBox[{"x", ",", "s", ",", "\[Phi]"}], ")"}]}]]}]}]}]}]}], 
   TraditionalForm]],
  FormatType->TraditionalForm,ExpressionUUID->
  "210ffede-389d-4f30-93d8-f69260e560fe"],
 ".\n- From Cartesian to curvilinear contravariant:\n",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SuperscriptBox["V", "j"], "=", 
    RowBox[{
