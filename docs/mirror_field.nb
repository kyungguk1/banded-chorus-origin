Cell[TextData[{
 "We want the center grid spacing along the field line to be proportional to ",
 Cell[BoxData[
  FormBox[
   RowBox[{"B", "[", 
    RowBox[{"x", ",", "0"}], "]"}], TraditionalForm]],ExpressionUUID->
  "c5b35ee8-c2c7-45d2-adef-7d0c5f094ede"],
 ", i.e., ",
 Cell[BoxData[
  FormBox[
   RowBox[{"\[CapitalDelta]x", "\[Proportional]", 
    RowBox[{"B", "[", 
     RowBox[{"x", ",", "0"}], "]"}]}], TraditionalForm]],ExpressionUUID->
  "32c40ece-5441-4550-9df7-896cebadd489"],
 ".\nIntegrating ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     SubscriptBox["\[CapitalDelta]", "1"], 
     RowBox[{"\[DifferentialD]", 
      SuperscriptBox["q", "1"]}]}], "=", 
    FractionBox[
     RowBox[{"\[DifferentialD]", "x"}], 
     RowBox[{"1", "+", 
      RowBox[{
       SuperscriptBox["\[Xi]", "2"], 
       SuperscriptBox["x", "2"]}]}]]}], TraditionalForm]],ExpressionUUID->
  "fd8d8b86-52f8-46a8-ab94-4e3f28a3b149"],
 ", ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     SubscriptBox["\[CapitalDelta]", "1"], 
     SuperscriptBox["q", "1"]}], "=", 
    RowBox[{
     RowBox[{
      FractionBox["1", "\[Xi]"], 
      SuperscriptBox["tan", 
       RowBox[{"-", "1"}]], "\[Xi]", " ", "x"}], "+", 
     "\[DoubleStruckCapitalC]"}]}], TraditionalForm]],ExpressionUUID->
  "750141b5-3150-48cd-9a5a-3487e994da77"],
 ".\nTo make ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     SuperscriptBox["q", "1"], "[", 
     RowBox[{"x", "=", "0"}], "]"}], "=", "0"}], TraditionalForm]],
  ExpressionUUID->"706e539f-24a8-40bf-8503-aa0f077d9a21"],
 ", we choose ",
 Cell[BoxData[
  FormBox[
   RowBox[{"\[DoubleStruckCapitalC]", "=", "0"}], TraditionalForm]],
  ExpressionUUID->"18ab6b9b-5551-4fa9-a6e0-bed1bd36e8c9"],
 ".\nThe horizontal grid spacing across the field line at the equator is \
constant.\nThen the curvilinear coordinates are\n",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"(", GridBox[{
       {
        RowBox[{
         SubscriptBox["\[CapitalDelta]", "1"], 
         SuperscriptBox["q", "1"]}]},
       {
        RowBox[{
         SubscriptBox["\[CapitalDelta]", "2"], 
         SuperscriptBox["q", "2"]}]}
      }], ")"}], "=", 
    RowBox[{"(", GridBox[{
       {
        RowBox[{
         FractionBox["1", "\[Xi]"], 
         SuperscriptBox["tan", 
          RowBox[{"-", "1"}]], "\[Xi]", " ", "x"}]},
       {
        RowBox[{
         RowBox[{"(", 
          RowBox[{"1", "+", 
           RowBox[{
            SuperscriptBox["\[Xi]", "2"], 
            SuperscriptBox["x", "2"]}]}], ")"}], "y"}]}
      }], ")"}]}], TraditionalForm]],ExpressionUUID->
  "38a6abcd-346b-4de6-a517-eed18520012a"],
 ", where ",
 Cell[BoxData[
  FormBox[
   SubscriptBox["\[CapitalDelta]", "1"], TraditionalForm]],ExpressionUUID->
  "65ef09b7-c450-434a-81e3-c75ebd924674"],
 " and ",
 Cell[BoxData[
  FormBox[
   SubscriptBox["\[CapitalDelta]", "2"], TraditionalForm]],ExpressionUUID->
  "4f7570be-98b7-4656-803b-8bd4cb94616d"],
 " are constant scale factors, and ",
 Cell[BoxData[
  FormBox[
   SuperscriptBox["q", "1"], TraditionalForm]],ExpressionUUID->
  "73b34baf-fcf9-4160-8e97-88d12fdfc56f"],
 " and ",
 Cell[BoxData[
  FormBox[
   SuperscriptBox["q", "2"], TraditionalForm]],ExpressionUUID->
  "86e71373-4a56-4739-8120-6ae146b8a5ec"],
 " are nondimensional.\nInverse of them are\n",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"(", GridBox[{
       {"x"},
       {"y"}
      }], ")"}], "=", 
    RowBox[{
     RowBox[{"(", GridBox[{
        {
         RowBox[{
          FractionBox["1", "\[Xi]"], 
          RowBox[{"tan", "[", 
           RowBox[{"\[Xi]", " ", 
            SubscriptBox["\[CapitalDelta]", "1"], 
            SuperscriptBox["q", "1"]}], "]"}]}]},
        {
         FractionBox[
          RowBox[{
           SubscriptBox["\[CapitalDelta]", "2"], 
           SuperscriptBox["q", "2"]}], 
          RowBox[{"(", 
           RowBox[{"1", "+", 
            RowBox[{
             SuperscriptBox["tan", "2"], "[", 
             RowBox[{"\[Xi]", " ", 
              SubscriptBox["\[CapitalDelta]", "1"], 
              SuperscriptBox["q", "1"]}], "]"}]}], ")"}]]}
       }], ")"}], "=", 
     RowBox[{"(", GridBox[{
        {
         RowBox[{
          FractionBox["1", "\[Xi]"], 
          RowBox[{"tan", "[", 
           RowBox[{"\[Xi]", " ", 
            SubscriptBox["\[CapitalDelta]", "1"], 
            SuperscriptBox["q", "1"]}], "]"}]}]},
        {
         RowBox[{
          SubscriptBox["\[CapitalDelta]", "2"], 
          SuperscriptBox["q", "2"], 
          RowBox[{
           SuperscriptBox["cos", "2"], "[", 
           RowBox[{"\[Xi]", " ", 
            SubscriptBox["\[CapitalDelta]", "1"], 
            SuperscriptBox["q", "1"]}], "]"}]}]}
       }], ")"}]}]}], TraditionalForm]],ExpressionUUID->
  "b49398fa-ea73-4f44-baed-90b2db0ce503"],
 ".\nIf ",
 Cell[BoxData[
  FormBox[
   RowBox[{"\[Xi]", "\[Rule]", "0"}], TraditionalForm]],ExpressionUUID->
  "22a82f68-8009-429a-8762-f040b4e07762"],
 ",\n",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"(", GridBox[{
       {"x"},
       {"y"}
      }], ")"}], "=", 
    RowBox[{"(", GridBox[{
       {
        RowBox[{
         SubscriptBox["\[CapitalDelta]", "1"], 
         SuperscriptBox["q", "1"]}]},
       {
        RowBox[{
         SubscriptBox["\[CapitalDelta]", "2"], 
         SuperscriptBox["q", "2"]}]}
      }], ")"}]}], TraditionalForm]],ExpressionUUID->
  "56818c3e-5a9c-40df-97d7-a74a06bc4264"],
 "."
}], "Text",ExpressionUUID->"84203e59-ea91-43c2-bed5-4ec6aea0b58a"],

Cell[BoxData[
 RowBox[{"With", "[", 
