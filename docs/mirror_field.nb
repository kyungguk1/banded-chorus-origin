Cell["Metric and Basis Vectors", "Subsection",ExpressionUUID->"d617778f-3a76-488a-acbd-97e5595bf711"],

Cell[TextData[{
 "Having established the relationship between the two different coordinate \
sets, let us now find the metric and basis vectors.\nThe covariant basis \
vectors are given by ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubscriptBox["\[DoubleStruckG]", "j"], "=", 
    FractionBox[
     RowBox[{"\[PartialD]", "\[DoubleStruckR]"}], 
     RowBox[{"\[PartialD]", 
      SuperscriptBox["q", "j"]}]]}], TraditionalForm]],ExpressionUUID->
  "5a6b936c-276b-45ce-ae0c-9696503f51fd"],
 ":\n",
 Cell[BoxData[{
  FormBox[
   RowBox[{
    SubscriptBox["\[DoubleStruckG]", "1"], "=", 
    RowBox[{
     FractionBox[
      RowBox[{"\[PartialD]", "\[DoubleStruckR]"}], 
      RowBox[{"\[PartialD]", 
       SuperscriptBox["q", "1"]}]], "=", 
     RowBox[{
      RowBox[{
       SubscriptBox["\[CapitalDelta]", "1"], 
       RowBox[{
        SuperscriptBox["sec", "2"], "(", 
        RowBox[{"\[Xi]", " ", 
         SubscriptBox["\[CapitalDelta]", "1"], 
         SuperscriptBox["q", "1"]}], ")"}], 
       SubscriptBox["\[DoubleStruckE]", "x"]}], "-", 
      RowBox[{
       SubscriptBox["\[CapitalDelta]", "1"], "\[Xi]", " ", 
       SubscriptBox["\[CapitalDelta]", "2"], 
       SuperscriptBox["q", "2"], 
       RowBox[{"sin", "(", 
        RowBox[{"\[Xi]", " ", 
         SubscriptBox["\[CapitalDelta]", "1"], 
         SuperscriptBox["q", "1"]}], ")"}], 
       SubscriptBox["\[DoubleStruckE]", "y"]}], "-", 
      RowBox[{
       SubscriptBox["\[CapitalDelta]", "1"], "\[Xi]", " ", 
       SubscriptBox["\[CapitalDelta]", "3"], 
       SuperscriptBox["q", "3"], 
       RowBox[{"sin", "(", 
        RowBox[{"\[Xi]", " ", 
         SubscriptBox["\[CapitalDelta]", "1"], 
         SuperscriptBox["q", "1"]}], ")"}], 
       SubscriptBox["\[DoubleStruckE]", "z"]}]}]}]}], 
   TraditionalForm], "\[IndentingNewLine]", 
  FormBox[
   RowBox[{"=", 
    RowBox[{
     SubscriptBox["\[CapitalDelta]", "1"], "(", 
     RowBox[{
      RowBox[{
       RowBox[{
        SuperscriptBox["sec", "2"], "(", 
        RowBox[{"\[Xi]", " ", 
         SubscriptBox["\[CapitalDelta]", "1"], 
         SuperscriptBox["q", "1"]}], ")"}], 
       SubscriptBox["\[DoubleStruckE]", "x"]}], "-", 
      RowBox[{"\[Xi]", " ", 
       SubscriptBox["\[CapitalDelta]", "2"], 
       SuperscriptBox["q", "2"], 
       RowBox[{"sin", "(", 
        RowBox[{"\[Xi]", " ", 
         SubscriptBox["\[CapitalDelta]", "1"], 
         SuperscriptBox["q", "1"]}], ")"}], 
       SubscriptBox["\[DoubleStruckE]", "y"]}], "-", 
      RowBox[{"\[Xi]", " ", 
       SubscriptBox["\[CapitalDelta]", "3"], 
       SuperscriptBox["q", "3"], 
       RowBox[{"sin", "(", 
        RowBox[{"\[Xi]", " ", 
         SubscriptBox["\[CapitalDelta]", "1"], 
         SuperscriptBox["q", "1"]}], ")"}], 
       SubscriptBox["\[DoubleStruckE]", "z"]}]}], ")"}]}], 
   TraditionalForm], "\[IndentingNewLine]", 
  FormBox[
   RowBox[{"=", 
    RowBox[{
     SubscriptBox["\[CapitalDelta]", "1"], "(", 
     RowBox[{
      RowBox[{
       RowBox[{"(", 
        RowBox[{"1", "+", 
         RowBox[{
          SuperscriptBox["\[Xi]", "2"], 
          SuperscriptBox["x", "2"]}]}], ")"}], 
       SubscriptBox["\[DoubleStruckE]", "x"]}], "-", 
      RowBox[{
       SuperscriptBox["\[Xi]", "2"], "x", " ", "y", " ", 
       SubscriptBox["\[DoubleStruckE]", "y"]}], "-", 
      RowBox[{
       SuperscriptBox["\[Xi]", "2"], "x", " ", "z", " ", 
       SubscriptBox["\[DoubleStruckE]", "z"]}]}], ")"}]}], 
   TraditionalForm], "\[IndentingNewLine]", 
  FormBox[
   RowBox[{"=", 
    RowBox[{
     SubscriptBox["\[CapitalDelta]", "1"], 
     FractionBox["\[DoubleStruckCapitalB]", 
      SubscriptBox["B", "0"]]}]}], TraditionalForm]}],ExpressionUUID->
  "f4d2c731-8820-4dea-a57d-6c0b7a1a615b"],
 ";\n",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubscriptBox["\[DoubleStruckG]", "2"], "=", 
    RowBox[{
     FractionBox[
      RowBox[{"\[PartialD]", "\[DoubleStruckR]"}], 
      RowBox[{"\[PartialD]", 
       SuperscriptBox["q", "2"]}]], "=", 
     RowBox[{
      RowBox[{
       SubscriptBox["\[CapitalDelta]", "2"], 
       RowBox[{"cos", "(", 
        RowBox[{"\[Xi]", " ", 
         SubscriptBox["\[CapitalDelta]", "1"], 
         SuperscriptBox["q", "1"]}], ")"}], 
       SubscriptBox["\[DoubleStruckE]", "y"]}], "=", 
      RowBox[{
       FractionBox[
        SubscriptBox["\[CapitalDelta]", "2"], 
        SqrtBox[
         RowBox[{"1", "+", 
          RowBox[{
           SuperscriptBox["\[Xi]", "2"], 
           SuperscriptBox["x", "2"]}]}]]], "=", 
       RowBox[{
        SubscriptBox["\[CapitalDelta]", "2"], 
        SqrtBox[
         FractionBox[
          SubscriptBox["B", "0"], 
          SubscriptBox["B", "x"]]], 
        SubscriptBox["\[DoubleStruckE]", "y"]}]}]}]}]}], TraditionalForm]],
  ExpressionUUID->"c8fde728-c51c-4edb-94c3-c057229af773"],
 ";\n",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubscriptBox["\[DoubleStruckG]", "3"], "=", 
    RowBox[{
     FractionBox[
      RowBox[{"\[PartialD]", "\[DoubleStruckR]"}], 
      RowBox[{"\[PartialD]", 
       SuperscriptBox["q", "3"]}]], "=", 
     RowBox[{
      RowBox[{
       SubscriptBox["\[CapitalDelta]", "3"], 
       RowBox[{"cos", "(", 
        RowBox[{"\[Xi]", " ", 
         SubscriptBox["\[CapitalDelta]", "1"], 
         SuperscriptBox["q", "1"]}], ")"}], 
       SubscriptBox["\[DoubleStruckE]", "z"]}], "=", 
      RowBox[{
       FractionBox[
        SubscriptBox["\[CapitalDelta]", "3"], 
        SqrtBox[
         RowBox[{"1", "+", 
          RowBox[{
           SuperscriptBox["\[Xi]", "2"], 
           SuperscriptBox["x", "2"]}]}]]], "=", 
       RowBox[{
        SubscriptBox["\[CapitalDelta]", "3"], 
        SqrtBox[
         FractionBox[
          SubscriptBox["B", "0"], 
          SubscriptBox["B", "x"]]], 
        SubscriptBox["\[DoubleStruckE]", "z"]}]}]}]}]}], TraditionalForm]],
  ExpressionUUID->"613c65ba-8dd1-4c4b-9cc8-73d0310c7d1d"],
 ";\nwhere we have used\n",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     SuperscriptBox["sec", "2"], "(", 
     RowBox[{"\[Xi]", " ", 
      SubscriptBox["\[CapitalDelta]", "1"], 
      SuperscriptBox["q", "1"]}], ")"}], "=", 
    RowBox[{
     RowBox[{"1", "+", 
      RowBox[{
       SuperscriptBox["\[Xi]", "2"], 
       SuperscriptBox["x", "2"]}]}], "=", 
     FractionBox[
      SubscriptBox["B", "x"], 
      SubscriptBox["B", "0"]]}]}], TraditionalForm]],ExpressionUUID->
  "63061d84-d9d7-4fee-bf76-85c5030ba26b"],
 ";\n",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     SuperscriptBox["sin", "2"], "(", 
     RowBox[{"\[Xi]", " ", 
      SubscriptBox["\[CapitalDelta]", "1"], 
      SuperscriptBox["q", "1"]}], ")"}], "=", 
    RowBox[{
     RowBox[{"1", "-", 
      FractionBox["1", 
       RowBox[{
        SuperscriptBox["sec", "2"], "(", 
        RowBox[{"\[Xi]", " ", 
         SubscriptBox["\[CapitalDelta]", "1"], 
         SuperscriptBox["q", "1"]}], ")"}]]}], "=", 
     FractionBox[
      RowBox[{
       SuperscriptBox["\[Xi]", "2"], 
       SuperscriptBox["x", "2"]}], 
      RowBox[{"1", "+", 
       RowBox[{
        SuperscriptBox["\[Xi]", "2"], 
        SuperscriptBox["x", "2"]}]}]]}]}], TraditionalForm]],ExpressionUUID->
  "3dc745b9-58f7-4816-8c14-00f946e1ad42"],
 ".\nThe covariant components of the metric tensor are\n",
 Cell[BoxData[{
  FormBox[
   RowBox[{
    SubscriptBox["g", 
     RowBox[{"i", "\[VeryThinSpace]", "j"}]], "=", 
    RowBox[{
     SubscriptBox["\[DoubleStruckG]", "i"], "\[CenterDot]", 
     SubscriptBox["\[DoubleStruckG]", "j"]}]}], 
   TraditionalForm], "\[IndentingNewLine]", 
  FormBox[
   RowBox[{"=", 
    RowBox[{
     RowBox[{"(", GridBox[{
        {
         RowBox[{
          SubsuperscriptBox["\[CapitalDelta]", "1", "2"], 
          FractionBox[
           SuperscriptBox["B", "2"], 
           SubsuperscriptBox["B", "0", "2"]]}], 
         RowBox[{
          SubscriptBox["\[CapitalDelta]", "1"], 
          SubscriptBox["\[CapitalDelta]", "2"], 
          SqrtBox[
           FractionBox[
            SubscriptBox["B", "0"], 
            SubscriptBox["B", "x"]]], 
          FractionBox[
           SubscriptBox["B", "y"], 
           SubscriptBox["B", "0"]]}], 
         RowBox[{
          SubscriptBox["\[CapitalDelta]", "1"], 
          SubscriptBox["\[CapitalDelta]", "3"], 
          SqrtBox[
           FractionBox[
            SubscriptBox["B", "0"], 
            SubscriptBox["B", "x"]]], 
          FractionBox[
           SubscriptBox["B", "z"], 
           SubscriptBox["B", "0"]]}]},
        {
         RowBox[{
          SubscriptBox["\[CapitalDelta]", "1"], 
          SubscriptBox["\[CapitalDelta]", "2"], 
          SqrtBox[
           FractionBox[
            SubscriptBox["B", "0"], 
            SubscriptBox["B", "x"]]], 
          FractionBox[
           SubscriptBox["B", "y"], 
           SubscriptBox["B", "0"]]}], 
         RowBox[{
          SubsuperscriptBox["\[CapitalDelta]", "2", "2"], 
          FractionBox[
           SubscriptBox["B", "0"], 
           SubscriptBox["B", "x"]]}], "0"},
        {
         RowBox[{
          SubscriptBox["\[CapitalDelta]", "1"], 
          SubscriptBox["\[CapitalDelta]", "3"], 
          SqrtBox[
           FractionBox[
            SubscriptBox["B", "0"], 
            SubscriptBox["B", "x"]]], 
          FractionBox[
           SubscriptBox["B", "z"], 
           SubscriptBox["B", "0"]]}], "0", 
         RowBox[{
          SubsuperscriptBox["\[CapitalDelta]", "3", "2"], 
          FractionBox[
           SubscriptBox["B", "0"], 
           SubscriptBox["B", "x"]]}]}
       }], ")"}], "=", 
     RowBox[{"(", GridBox[{
        {
         RowBox[{
          SubsuperscriptBox["\[CapitalDelta]", "1", "2"], "(", 
          RowBox[{
           SuperscriptBox[
            RowBox[{"(", 
             RowBox[{"1", "+", 
              RowBox[{
               SuperscriptBox["\[Xi]", "2"], 
               SuperscriptBox["x", "2"]}]}], ")"}], "2"], "+", 
           RowBox[{
            SuperscriptBox["\[Xi]", "4"], 
            SuperscriptBox["x", "2"], 
            SuperscriptBox["s", "2"]}]}], ")"}], 
         RowBox[{
          SubscriptBox["\[CapitalDelta]", "1"], 
          SubscriptBox["\[CapitalDelta]", "2"], 
          FractionBox[
           RowBox[{
            RowBox[{"-", 
             SuperscriptBox["\[Xi]", "2"]}], "x", " ", "y"}], 
           SqrtBox[
            RowBox[{"1", "+", 
             RowBox[{
              SuperscriptBox["\[Xi]", "2"], 
              SuperscriptBox["x", "2"]}]}]]]}], 
         RowBox[{
          SubscriptBox["\[CapitalDelta]", "1"], 
          SubscriptBox["\[CapitalDelta]", "3"], 
          FractionBox[
           RowBox[{
            RowBox[{"-", 
             SuperscriptBox["\[Xi]", "2"]}], "x", " ", "z"}], 
           SqrtBox[
            RowBox[{"1", "+", 
             RowBox[{
              SuperscriptBox["\[Xi]", "2"], 
              SuperscriptBox["x", "2"]}]}]]]}]},
        {
         RowBox[{
          SubscriptBox["\[CapitalDelta]", "1"], 
          SubscriptBox["\[CapitalDelta]", "2"], 
          FractionBox[
           RowBox[{
            RowBox[{"-", 
             SuperscriptBox["\[Xi]", "2"]}], "x", " ", "y"}], 
           SqrtBox[
            RowBox[{"1", "+", 
             RowBox[{
              SuperscriptBox["\[Xi]", "2"], 
              SuperscriptBox["x", "2"]}]}]]]}], 
         RowBox[{
          SubsuperscriptBox["\[CapitalDelta]", "2", "2"], 
          FractionBox["1", 
           RowBox[{"1", "+", 
            RowBox[{
             SuperscriptBox["\[Xi]", "2"], 
             SuperscriptBox["x", "2"]}]}]]}], "0"},
        {
         RowBox[{
          SubscriptBox["\[CapitalDelta]", "1"], 
          SubscriptBox["\[CapitalDelta]", "3"], 
          FractionBox[
           RowBox[{
            RowBox[{"-", 
             SuperscriptBox["\[Xi]", "2"]}], "x", " ", "z"}], 
           SqrtBox[
            RowBox[{"1", "+", 
             RowBox[{
              SuperscriptBox["\[Xi]", "2"], 
              SuperscriptBox["x", "2"]}]}]]]}], "0", 
         RowBox[{
          SubsuperscriptBox["\[CapitalDelta]", "3", "2"], 
          FractionBox["1", 
           RowBox[{"1", "+", 
            RowBox[{
             SuperscriptBox["\[Xi]", "2"], 
             SuperscriptBox["x", "2"]}]}]]}]}
       }], ")"}]}]}], TraditionalForm], "\[IndentingNewLine]", 
  FormBox[
   RowBox[{"=", 
    RowBox[{"(", GridBox[{
       {
        RowBox[{
         SubsuperscriptBox["\[CapitalDelta]", "1", "2"], "(", 
         RowBox[{
          RowBox[{
           SuperscriptBox["sec", "4"], 
           RowBox[{"(", 
            RowBox[{"\[Xi]", " ", 
             SubscriptBox["\[CapitalDelta]", "1"], 
             SuperscriptBox["q", "1"]}], ")"}]}], "+", 
          RowBox[{
           RowBox[{"(", 
            RowBox[{
             SuperscriptBox[
              RowBox[{"(", 
               RowBox[{"\[Xi]", " ", 
                SubscriptBox["\[CapitalDelta]", "2"], 
                SuperscriptBox["q", "2"]}], ")"}], "2"], "+", 
             SuperscriptBox[
              RowBox[{"(", 
               RowBox[{"\[Xi]", " ", 
                SubscriptBox["\[CapitalDelta]", "3"], 
                SuperscriptBox["q", "3"]}], ")"}], "2"]}], ")"}], 
           RowBox[{
            SuperscriptBox["sin", "2"], "(", 
            RowBox[{"\[Xi]", " ", 
             SubscriptBox["\[CapitalDelta]", "1"], 
             SuperscriptBox["q", "1"]}], ")"}]}]}], ")"}], 
        RowBox[{
         RowBox[{"-", 
          SubscriptBox["\[CapitalDelta]", "1"]}], 
         SubscriptBox["\[CapitalDelta]", "2"], 
         FractionBox[
          RowBox[{"\[Xi]", " ", 
           SubscriptBox["\[CapitalDelta]", "2"], 
           SuperscriptBox["q", "2"]}], "2"], 
         RowBox[{"sin", "(", 
          RowBox[{"2", "\[Xi]", " ", 
           SubscriptBox["\[CapitalDelta]", "1"], 
           SuperscriptBox["q", "1"]}], ")"}]}], 
        RowBox[{
         RowBox[{"-", 
          SubscriptBox["\[CapitalDelta]", "1"]}], 
         SubscriptBox["\[CapitalDelta]", "3"], 
         FractionBox[
          RowBox[{"\[Xi]", " ", 
           SubscriptBox["\[CapitalDelta]", "3"], 
           SuperscriptBox["q", "3"]}], "2"], 
         RowBox[{"sin", "(", 
          RowBox[{"2", "\[Xi]", " ", 
           SubscriptBox["\[CapitalDelta]", "1"], 
           SuperscriptBox["q", "1"]}], ")"}]}]},
       {
        RowBox[{
         RowBox[{"-", 
          SubscriptBox["\[CapitalDelta]", "1"]}], 
         SubscriptBox["\[CapitalDelta]", "2"], 
         FractionBox[
          RowBox[{"\[Xi]", " ", 
           SubscriptBox["\[CapitalDelta]", "2"], 
           SuperscriptBox["q", "2"]}], "2"], "sin", 
         RowBox[{"(", 
          RowBox[{"2", "\[Xi]", " ", 
           SubscriptBox["\[CapitalDelta]", "1"], 
           SuperscriptBox["q", "1"]}], ")"}]}], 
        RowBox[{
         SubsuperscriptBox["\[CapitalDelta]", "2", "2"], 
         RowBox[{
          SuperscriptBox["cos", "2"], "(", 
          RowBox[{"\[Xi]", " ", 
           SubscriptBox["\[CapitalDelta]", "1"], 
           SuperscriptBox["q", "1"]}], ")"}]}], "0"},
       {
        RowBox[{
         RowBox[{"-", 
          SubscriptBox["\[CapitalDelta]", "1"]}], 
         SubscriptBox["\[CapitalDelta]", "3"], 
         FractionBox[
          RowBox[{"\[Xi]", " ", 
           SubscriptBox["\[CapitalDelta]", "3"], 
           SuperscriptBox["q", "3"]}], "2"], "sin", 
         RowBox[{"(", 
          RowBox[{"2", "\[Xi]", " ", 
           SubscriptBox["\[CapitalDelta]", "1"], 
           SuperscriptBox["q", "1"]}], ")"}]}], "0", 
        RowBox[{
         SubsuperscriptBox["\[CapitalDelta]", "3", "2"], 
         RowBox[{
          SuperscriptBox["cos", "2"], "(", 
          RowBox[{"\[Xi]", " ", 
           SubscriptBox["\[CapitalDelta]", "1"], 
           SuperscriptBox["q", "1"]}], ")"}]}]}
      }], ")"}]}], TraditionalForm]}],ExpressionUUID->
  "f92e1de6-7475-4a45-bc80-da68090b7b98"],
 ".\nNote that\n",
 Cell[BoxData[
  FormBox[
   RowBox[{"g", "=", 
    RowBox[{
     RowBox[{"\[LeftBracketingBar]", 
      SubscriptBox["g", 
       RowBox[{"i", "\[VeryThinSpace]", "j"}]], "\[RightBracketingBar]"}], 
     "=", 
