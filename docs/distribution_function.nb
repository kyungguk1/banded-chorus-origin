Cell[BoxData[
 RowBox[{"With", "[", 
  RowBox[{
   RowBox[{"{", 
    RowBox[{
     RowBox[{"c", "=", "100."}], ",", 
     RowBox[{"P2OP1", "=", "10."}], ",", 
     RowBox[{"Vd", "=", "10."}]}], 
    RowBox[{"(*", 
     RowBox[{"along", " ", "the", " ", "first", " ", "dim"}], "*)"}], "}"}], 
   ",", "\[IndentingNewLine]", 
   RowBox[{
    RowBox[{"If", "[", 
     RowBox[{
      RowBox[{"c", "<=", 
       RowBox[{"Abs", "[", "Vd", "]"}]}], ",", 
      RowBox[{"(", 
       RowBox[{
        RowBox[{"Print", "[", "\"\<Vd exceeding c\>\"", "]"}], ";", 
        RowBox[{"Abort", "[", "]"}]}], ")"}]}], "]"}], ";", 
    "\[IndentingNewLine]", 
    RowBox[{"Module", "[", 
     RowBox[{
      RowBox[{"{", 
       RowBox[{
       "\[Gamma]d", ",", "f0", ",", "n0", ",", "\[Rho]00", ",", "P0ij", ",", 
