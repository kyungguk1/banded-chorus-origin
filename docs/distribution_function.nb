Cell[CellGroupData[{

Cell["The equation of state", "Subsubsection",ExpressionUUID->"e2c41bba-5877-4f0c-9b22-1b8143c9d47b"],

Cell[TextData[{
 "In the nonrelativistic limit ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubscriptBox["\[CurlyEpsilon]", "0"], "=", 
    RowBox[{
     SubscriptBox["m", "0"], "(", 
     RowBox[{
      SuperscriptBox["c", "2"], "+", 
      RowBox[{
       FractionBox["1", "2"], 
       SubsuperscriptBox["\[Theta]", "0", "2"]}]}], ")"}]}], 
   TraditionalForm]],ExpressionUUID->"47f27575-fd08-4448-a7ac-b41ca95abaca"],
 ", one easily find\n",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     SubscriptBox["\[Rho]", "00"], 
     SuperscriptBox["c", "2"]}], "=", 
    RowBox[{
     RowBox[{"\[Integral]", 
      RowBox[{
       RowBox[{
        SubscriptBox["m", "0"], "(", 
        RowBox[{
         SuperscriptBox["c", "2"], "+", 
         RowBox[{
          FractionBox["1", "2"], 
          SubsuperscriptBox["\[Theta]", "0", "2"]}]}], ")"}], 
       RowBox[{
        SubscriptBox["f", "0"], "(", 
        SubscriptBox[
         OverscriptBox["\[Theta]", "\[RightVector]"], "0"], ")"}], 
       RowBox[{
        SuperscriptBox["\[DifferentialD]", "3"], 
        SubscriptBox["\[Theta]", "0"]}]}]}], "=", 
     RowBox[{
      RowBox[{
       SubscriptBox["\[Rho]", "0"], 
       SuperscriptBox["c", "2"]}], "+", 
      RowBox[{
       FractionBox["1", "2"], 
       RowBox[{
        SubscriptBox["n", "0"], "(", 
        RowBox[{
         SubscriptBox["T", "\[DoubleVerticalBar]"], "+", 
         RowBox[{"2", 
          SubscriptBox["T", "\[UpTee]"]}]}], ")"}]}]}]}]}], TraditionalForm]],
  ExpressionUUID->"0aae14db-6a48-41cd-9ba9-e42d03e4d6be"],
 ",\nwhere ",
 Cell[BoxData[
  FormBox["T", TraditionalForm]],ExpressionUUID->
  "ca945715-4cc3-4a3f-af7d-8f260e370cdf"],
 " is the temperature measured in the comoving frame. Hence the specific\n\
internal energy is ",
 Cell[BoxData[
  FormBox[
   RowBox[{"e", "=", 
    RowBox[{
     FractionBox["1", "2"], 
     RowBox[{
      RowBox[{
       SubscriptBox["n", "0"], "(", 
       RowBox[{
        SubscriptBox["T", "\[DoubleVerticalBar]"], "+", 
        RowBox[{"2", 
         SubscriptBox["T", "\[UpTee]"]}]}], ")"}], "/", 
      SubscriptBox["m", "0"]}]}]}], TraditionalForm]],ExpressionUUID->
  "0dbae5d0-756e-41ea-9c31-19a6f8525705"],
 " and the energy per unit volume is ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    FractionBox["1", "2"], 
    RowBox[{
     SubscriptBox["n", "0"], "(", 
     RowBox[{
      SubscriptBox["T", "\[DoubleVerticalBar]"], "+", 
      RowBox[{"2", 
       SubscriptBox["T", "\[UpTee]"]}]}], ")"}]}], TraditionalForm]],
  ExpressionUUID->"3f770700-4372-4b83-971f-dbb372bd9307"],
 ".\nSimilarly, in the nonrelativistic limit ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubsuperscriptBox["\[Theta]", "0", "i"], "=", 
    RowBox[{
     FractionBox[
      SubsuperscriptBox["p", "0", "i"], 
      RowBox[{
       SubscriptBox["m", "0"], "\[Gamma]"}]], "=", 
     RowBox[{
      SubsuperscriptBox["\[Theta]", "0", "i"], "(", 
      RowBox[{"1", "-", 
       RowBox[{
        FractionBox["1", "2"], 
        FractionBox[
         SubsuperscriptBox["\[Theta]", "0", "2"], 
         SuperscriptBox["c", "2"]]}]}], ")"}]}]}], TraditionalForm]],
  ExpressionUUID->"eaa364df-c4f8-4c32-9c1a-e93b5876493e"],
 " and\n",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubsuperscriptBox["P", "0", 
     RowBox[{"i", "\[VeryThinSpace]", "j"}]], "=", 
    RowBox[{
     RowBox[{"\[Integral]", 
      RowBox[{
       SubsuperscriptBox["\[Theta]", "0", "i"], 
       SubsuperscriptBox["p", "0", "j"], 
       RowBox[{
        SubscriptBox["f", "0"], "(", 
        SubscriptBox["\[DoubleStruckP]", "0"], ")"}], 
       RowBox[{"\[DifferentialD]", 
        SubscriptBox["\[DoubleStruckP]", "0"]}]}]}], "=", 
     RowBox[{
      SubscriptBox["m", "0"], 
      RowBox[{"\[Integral]", 
       RowBox[{
        RowBox[{"(", 
         RowBox[{
          RowBox[{
           SubsuperscriptBox["\[Theta]", "0", "i"], 
           SubsuperscriptBox["\[Theta]", "0", "j"]}], "-", 
          RowBox[{
           FractionBox["1", "2"], 
           FractionBox[
            SubsuperscriptBox["\[Theta]", "0", "2"], 
            SuperscriptBox["c", "2"]], 
           SubsuperscriptBox["\[Theta]", "0", "i"], 
           SubsuperscriptBox["\[Theta]", "0", "j"]}]}], ")"}], 
        RowBox[{
         SubscriptBox["f", "0"], "(", 
         SubscriptBox[
          OverscriptBox["\[Theta]", "\[RightVector]"], "0"], ")"}], 
        RowBox[{
         SuperscriptBox["\[DifferentialD]", "3"], 
         SubscriptBox["\[Theta]", "0"]}]}]}]}]}]}], TraditionalForm]],
  ExpressionUUID->"0a8ca0a2-0e6c-4bdc-9815-21f5d3cee730"],
 "."
}], "Text",ExpressionUUID->"c6b6e89f-ac0a-4bd3-b14a-ec1a5cdebea6"]
