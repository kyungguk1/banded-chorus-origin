      "\[IndentingNewLine]", 
      RowBox[{
       RowBox[{"\[Gamma]d", "=", 
        RowBox[{"1", "/", 
         RowBox[{"Sqrt", "[", 
          RowBox[{
           RowBox[{"(", 
            RowBox[{"1", "-", 
             RowBox[{"Vd", "/", "c"}]}], ")"}], "*", 
           RowBox[{"(", 
            RowBox[{"1", "+", 
             RowBox[{"Vd", "/", "c"}]}], ")"}]}], "]"}]}]}], ";", 
       "\[IndentingNewLine]", 
       RowBox[{"f0", "=", 
        RowBox[{"Function", "[", 
         RowBox[{
          RowBox[{"{", 
           RowBox[{"u1", ",", "u2"}], "}"}], ",", 
          RowBox[{"Divide", "[", 
           RowBox[{
            RowBox[{"Exp", "[", 
             RowBox[{"-", 
              RowBox[{"(", 
               RowBox[{
                RowBox[{"u1", "^", "2"}], "+", 
                RowBox[{
                 RowBox[{"u2", "^", "2"}], "/", "P2OP1"}]}], ")"}]}], "]"}], 
            ",", 
            RowBox[{
             RowBox[{"Pi", "^", "1.5"}], "P2OP1"}]}], "]"}]}], "]"}]}], ";", 
       "\[IndentingNewLine]", 
       RowBox[{"n0", "=", 
        RowBox[{"NIntegrate", "[", 
         RowBox[{
          RowBox[{
           RowBox[{"f0", "[", 
            RowBox[{"u1", ",", "u2"}], "]"}], "2", "Pi", " ", "u2"}], ",", 
          RowBox[{"{", 
           RowBox[{"u1", ",", 
            RowBox[{"-", "5"}], ",", "5"}], "}"}], ",", 
          RowBox[{"{", 
           RowBox[{"u2", ",", "0", ",", 
            RowBox[{"5", 
             SqrtBox["P2OP1"]}]}], "}"}]}], "]"}]}], ";", 
       "\[IndentingNewLine]", 
       RowBox[{"\[Rho]00", "=", 
        RowBox[{"NIntegrate", "[", 
         RowBox[{
          RowBox[{
           RowBox[{"(", 
            RowBox[{"Sqrt", "[", 
             RowBox[{"1", "+", 
              RowBox[{
               RowBox[{"(", 
                RowBox[{
                 RowBox[{"u1", "^", "2"}], "+", 
                 RowBox[{"u2", "^", "2"}]}], ")"}], "/", 
               RowBox[{"c", "^", "2"}]}]}], "]"}], ")"}], 
           RowBox[{"f0", "[", 
            RowBox[{"u1", ",", "u2"}], "]"}], "2", "Pi", " ", "u2"}], ",", 
          RowBox[{"{", 
           RowBox[{"u1", ",", 
            RowBox[{"-", "5"}], ",", "5"}], "}"}], ",", 
          RowBox[{"{", 
           RowBox[{"u2", ",", "0", ",", 
            RowBox[{"5", 
             SqrtBox["P2OP1"]}]}], "}"}]}], "]"}]}], ";", 
       "\[IndentingNewLine]", 
       RowBox[{"P0ij", "=", 
        RowBox[{"NIntegrate", "[", 
         RowBox[{
          RowBox[{
           RowBox[{"(", 
            RowBox[{
             RowBox[{"{", 
              RowBox[{
               RowBox[{"u1", "^", "2"}], ",", 
               RowBox[{
                RowBox[{"u2", "^", "2"}], "/", "2"}]}], "}"}], "/", 
             RowBox[{"Sqrt", "[", 
              RowBox[{"1", "+", 
               RowBox[{
                RowBox[{"(", 
                 RowBox[{
                  RowBox[{"u1", "^", "2"}], "+", 
                  RowBox[{"u2", "^", "2"}]}], ")"}], "/", 
                RowBox[{"c", "^", "2"}]}]}], "]"}]}], ")"}], 
           RowBox[{"f0", "[", 
            RowBox[{"u1", ",", "u2"}], "]"}], "2", "Pi", " ", "u2"}], ",", 
          RowBox[{"{", 
           RowBox[{"u1", ",", 
            RowBox[{"-", "5"}], ",", "5"}], "}"}], ",", 
          RowBox[{"{", 
           RowBox[{"u2", ",", "0", ",", 
            RowBox[{"5", 
