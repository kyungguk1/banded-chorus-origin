                  RowBox[{"c", "^", "2"}]}]}], "]"}]}], ")"}], 
             RowBox[{"f", "[", 
              RowBox[{"u1", ",", "u2"}], "]"}], "2", "Pi", " ", "u2"}], ",", 
            RowBox[{"{", 
             RowBox[{"u1", ",", 
              RowBox[{
               RowBox[{"-", "5"}], "+", 
               RowBox[{"\[Gamma]d", " ", "Vd"}]}], ",", 
              RowBox[{"5", "+", 
               RowBox[{"\[Gamma]d", " ", "Vd"}]}]}], "}"}], ",", 
            RowBox[{"{", 
             RowBox[{"u2", ",", "0", ",", 
              RowBox[{"5", 
               SqrtBox["P2OP1"]}]}], "}"}]}], "]"}]}], "\[IndentingNewLine]", 
         "]"}]}], ";", "\[IndentingNewLine]", 
       RowBox[{"Print", "[", 
        RowBox[{"{", 
         RowBox[{
          RowBox[{"nV", "/", "n"}], ",", "Vd"}], "}"}], "]"}], ";", 
       "\[IndentingNewLine]", 
       RowBox[{"ED1", "=", 
        RowBox[{"NIntegrate", "[", 
         RowBox[{
          RowBox[{
           RowBox[{"(", 
            RowBox[{
             RowBox[{"c", "^", "2"}], 
             RowBox[{"Sqrt", "[", 
              RowBox[{"1", "+", 
               RowBox[{
                RowBox[{"(", 
                 RowBox[{
                  RowBox[{"u1", "^", "2"}], "+", 
                  RowBox[{"u2", "^", "2"}]}], ")"}], "/", 
