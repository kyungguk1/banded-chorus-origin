   RowBox[{"ED", "\[TildeTilde]", 
    RowBox[{
     SuperscriptBox["\[Gamma]", "2"], 
     SubscriptBox["\[Rho]", "00"], 
     SuperscriptBox["c", "2"]}]}], TraditionalForm]],
  FormatType->TraditionalForm,ExpressionUUID->
  "98bc78da-50a2-4a33-afcc-d4471d071214"],
 ".\nThe momentum density in the lab frame:\n",
 Cell[BoxData[{
  FormBox[
   RowBox[{"MD", "=", 
    RowBox[{
     RowBox[{"\[Integral]", 
      RowBox[{"\[DoubleStruckP]", " ", 
       RowBox[{"f", "(", "\[DoubleStruckP]", ")"}], 
       RowBox[{"\[DifferentialD]", "\[DoubleStruckP]"}]}]}], "=", 
     RowBox[{"\[Integral]", 
      RowBox[{
       RowBox[{"(", 
        RowBox[{"\[CurlyEpsilon]", "/", 
         SubscriptBox["\[CurlyEpsilon]", "0"]}], ")"}], "\[DoubleStruckP]", 
       " ", 
       RowBox[{
        SubscriptBox["f", "0"], "(", 
        SubscriptBox["\[DoubleStruckP]", "0"], ")"}], 
       RowBox[{"\[DifferentialD]", 
        SubscriptBox["\[DoubleStruckP]", "0"]}]}]}]}]}], 
   TraditionalForm], "\[IndentingNewLine]", 
  FormBox[
   RowBox[{"=", 
    RowBox[{"\[Gamma]", 
     RowBox[{"\[Integral]", 
      RowBox[{
       RowBox[{"(", 
        RowBox[{"1", "+", 
         RowBox[{"\[DoubleStruckCapitalV]", "\[CenterDot]", 
