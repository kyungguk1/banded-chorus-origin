       RowBox[{
        SubscriptBox["f", "0"], "(", 
        SubscriptBox["\[DoubleStruckP]", "0"], ")"}], 
       RowBox[{"\[DifferentialD]", 
        SubscriptBox["\[DoubleStruckP]", "0"]}]}]}]}]}], 
   TraditionalForm], "\[IndentingNewLine]", 
  FormBox[
   RowBox[{"=", 
    RowBox[{
     SuperscriptBox["\[Gamma]", "2"], 
     RowBox[{"\[Integral]", 
      RowBox[{
       RowBox[{"(", 
        RowBox[{
         SubscriptBox["\[CurlyEpsilon]", "0"], "+", 
         RowBox[{
          SuperscriptBox[
           RowBox[{"(", 
            RowBox[{"\[DoubleStruckCapitalV]", "\[CenterDot]", 
             SubscriptBox["\[DoubleStruckP]", "0"]}], ")"}], "2"], "/", 
          SubscriptBox["\[CurlyEpsilon]", "0"]}]}], ")"}], 
       RowBox[{
        SubscriptBox["f", "0"], "(", 
        SubscriptBox["\[DoubleStruckP]", "0"], ")"}], 
       RowBox[{"\[DifferentialD]", 
        SubscriptBox["\[DoubleStruckP]", "0"]}]}]}]}]}], TraditionalForm]}],
  FormatType->TraditionalForm,ExpressionUUID->
  "3fd3ae72-7c24-4a4e-9a4c-70da87497a7b"],
 ",\nwhere the assumption of symmetric ",
 Cell[BoxData[
  FormBox[
   SubscriptBox["f", "0"], TraditionalForm]],
  FormatType->TraditionalForm,ExpressionUUID->
  "05c2b83e-0946-4009-a4e3-e54f894ced3b"],
 " is made.\nUsing ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubscriptBox["\[DoubleStruckP]", "0"], "=", 
    RowBox[{
     SubscriptBox["\[CurlyEpsilon]", "0"], 
     RowBox[{
      SubscriptBox[
       OverscriptBox["\[Theta]", "\[RightVector]"], "0"], "/", 
      SuperscriptBox["c", "2"]}]}]}], TraditionalForm]],
  FormatType->TraditionalForm,ExpressionUUID->
  "4d1b7a66-4c70-44ec-a922-c6942c2a363c"],
 ",\n",
 Cell[BoxData[{
  FormBox[
   RowBox[{"ED", "=", 
    RowBox[{
     SuperscriptBox["\[Gamma]", "2"], 
     RowBox[{"\[Integral]", 
      RowBox[{
       RowBox[{"(", 
        RowBox[{
         SubscriptBox["\[CurlyEpsilon]", "0"], "+", 
         RowBox[{
          RowBox[{"(", 
           RowBox[{"\[DoubleStruckCapitalV]", "\[CenterDot]", 
            SubscriptBox["\[DoubleStruckP]", "0"]}], ")"}], 
          RowBox[{
           RowBox[{"(", 
            RowBox[{"\[DoubleStruckCapitalV]", "\[CenterDot]", 
             SubscriptBox[
              OverscriptBox["\[Theta]", "\[RightVector]"], "0"]}], ")"}], "/", 
           SuperscriptBox["c", "2"]}]}]}], ")"}], 
       RowBox[{
        SubscriptBox["f", "0"], "(", 
        SubscriptBox["\[DoubleStruckP]", "0"], ")"}], 
       RowBox[{"\[DifferentialD]", 
        SubscriptBox["\[DoubleStruckP]", "0"]}]}]}]}]}], 
   TraditionalForm], "\[IndentingNewLine]", 
  FormBox[
   RowBox[{"=", 
    RowBox[{
     RowBox[{
      SuperscriptBox["\[Gamma]", "2"], 
      RowBox[{"\[Integral]", 
       RowBox[{
        SubscriptBox["\[CurlyEpsilon]", "0"], 
