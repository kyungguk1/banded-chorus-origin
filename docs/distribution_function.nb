Cell[CellGroupData[{
Cell["Macroscopic Variables in Relativistic Regime", "Section",ExpressionUUID->"c66cbf6b-9945-4d00-ae81-0fe3a778159d"],

Cell[CellGroupData[{

Cell["Lorentz Transformation", "Subsection",ExpressionUUID->"4e8933b4-33fa-4771-8784-1f3dcf2f458b"],

Cell[TextData[{
 "Suppose that ",
 Cell[BoxData[
  FormBox[
   RowBox[{"S", "'"}], TraditionalForm]],ExpressionUUID->
  "b7082296-580a-42e4-8de8-c0735b213d6b"],
 " moves with a velocity \[DoubleStruckV] relative to ",
 Cell[BoxData[
  FormBox["S", TraditionalForm]],ExpressionUUID->
  "ea045bba-1a1d-4ff8-abfe-1932c842333b"],
 ". The homogeneous Lorentz\ntransformation of ",
 Cell[BoxData[
  FormBox[
   RowBox[{"(", 
    RowBox[{
     RowBox[{"c", " ", "t"}], ",", "\[DoubleStruckX]"}], ")"}], 
   TraditionalForm]],ExpressionUUID->"cfb8aa24-2de0-4cc7-800e-3ca535efeb74"],
 " can be written as\n",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"(", GridBox[{
       {
        RowBox[{"c", " ", 
         RowBox[{"t", "'"}]}]},
       {
        RowBox[{"\[DoubleStruckX]", "'"}]}
      }], ")"}], "=", 
    RowBox[{
     RowBox[{"(", GridBox[{
        {"\[Gamma]", 
         RowBox[{
          RowBox[{"-", "\[Gamma]"}], " ", 
          RowBox[{"\[DoubleStruckV]", "/", "c"}]}]},
        {
         RowBox[{
          RowBox[{"-", "\[Gamma]"}], " ", 
          RowBox[{"\[DoubleStruckV]", "/", "c"}]}], 
         RowBox[{"\[DoubleStruckCapitalI]", "+", 
          RowBox[{
           RowBox[{"(", 
            RowBox[{"\[Gamma]", "-", "1"}], ")"}], 
           SuperscriptBox["v", 
            RowBox[{"-", "2"}]], "\[DoubleStruckV]\[DoubleStruckV]"}]}]}
       }], ")"}], 
     RowBox[{"(", GridBox[{
        {
         RowBox[{"c", " ", "t"}]},
        {"\[DoubleStruckX]"}
       }], ")"}]}]}], TraditionalForm]],ExpressionUUID->
  "23d5e1e6-2f52-435d-810b-28301e5d1e49"],
 ",\nor, equivalently,\n",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"c", " ", 
     RowBox[{"t", "'"}]}], "=", 
    RowBox[{"\[Gamma]", "(", 
     RowBox[{
      RowBox[{"c", " ", "t"}], "-", 
      RowBox[{
       RowBox[{"(", 
        RowBox[{"\[DoubleStruckV]", "/", "c"}], ")"}], "\[CenterDot]", 
       "\[DoubleStruckX]"}]}], ")"}]}], TraditionalForm]],ExpressionUUID->
  "126bc192-7e9f-481a-973a-8c8e658aacbd"],
 " \nand\n",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"\[DoubleStruckX]", "'"}], "=", 
    RowBox[{"\[DoubleStruckX]", "+", 
     RowBox[{
      RowBox[{"(", 
       RowBox[{"\[Gamma]", "-", "1"}], ")"}], 
      RowBox[{"(", 
       RowBox[{"\[DoubleStruckV]", "\[CenterDot]", "\[DoubleStruckX]"}], 
       ")"}], 
      RowBox[{"\[DoubleStruckV]", "/", 
       SuperscriptBox["v", "2"]}]}], "-", 
     RowBox[{"\[Gamma]", " ", 
      RowBox[{"(", 
       RowBox[{"c", " ", "t"}], ")"}], 
      RowBox[{"(", 
       RowBox[{"\[DoubleStruckV]", "/", "c"}], ")"}]}]}]}], TraditionalForm]],
  ExpressionUUID->"b1025e54-7992-4b60-9e3b-bb41efff5941"],
 ".\nIn general, Lorentz transformation of a four-vector ",
 Cell[BoxData[
  FormBox[
   SuperscriptBox["A", "i"], TraditionalForm]],ExpressionUUID->
  "98627443-bd3a-49ed-8f8e-05e2b37893d9"],
 " is given by\n",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"(", GridBox[{
       {
        RowBox[{
         SuperscriptBox["A", "0"], "'"}]},
       {
        RowBox[{"\[DoubleStruckCapitalA]", "'"}]}
      }], ")"}], "=", 
    RowBox[{
     RowBox[{"(", GridBox[{
        {"\[Gamma]", 
         RowBox[{
          RowBox[{"-", "\[Gamma]"}], " ", 
          RowBox[{"\[DoubleStruckV]", "/", "c"}]}]},
        {
         RowBox[{
          RowBox[{"-", "\[Gamma]"}], " ", 
          RowBox[{"\[DoubleStruckV]", "/", "c"}]}], 
         RowBox[{"\[DoubleStruckCapitalI]", "+", 
          RowBox[{
           RowBox[{"(", 
            RowBox[{"\[Gamma]", "-", "1"}], ")"}], 
           SuperscriptBox["v", 
            RowBox[{"-", "2"}]], "\[DoubleStruckV]\[DoubleStruckV]"}]}]}
       }], ")"}], 
     RowBox[{"(", GridBox[{
        {
         SuperscriptBox["A", "0"]},
        {"\[DoubleStruckCapitalA]"}
       }], ")"}]}]}], TraditionalForm]],ExpressionUUID->
  "cc375a75-5a28-43b1-a637-962964949bee"],
 ",\nor, equivalently,\n",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     SuperscriptBox["A", "0"], "'"}], "=", 
    RowBox[{"\[Gamma]", "(", 
     RowBox[{
      SuperscriptBox["A", "0"], "-", 
      RowBox[{
       RowBox[{"(", 
        RowBox[{"\[DoubleStruckV]", "/", "c"}], ")"}], "\[CenterDot]", 
       "\[DoubleStruckCapitalA]"}]}], ")"}]}], TraditionalForm]],
  ExpressionUUID->"77c4d103-a272-44c3-a54b-041a15293753"],
 " \nand\n",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"\[DoubleStruckCapitalA]", "'"}], "=", 
    RowBox[{"\[DoubleStruckCapitalA]", "+", 
     RowBox[{
      RowBox[{"(", 
       RowBox[{"\[Gamma]", "-", "1"}], ")"}], 
      RowBox[{"(", 
       RowBox[{
       "\[DoubleStruckV]", "\[CenterDot]", "\[DoubleStruckCapitalA]"}], ")"}], 
      RowBox[{"\[DoubleStruckV]", "/", 
       SuperscriptBox["v", "2"]}]}], "-", 
     RowBox[{"\[Gamma]", " ", 
      RowBox[{
       SuperscriptBox["A", "0"], "(", 
       RowBox[{"\[DoubleStruckV]", "/", "c"}], ")"}]}]}]}], TraditionalForm]],
  ExpressionUUID->"214471c4-ac6f-47c0-8dcb-e2f6808346a9"],
 "."
}], "Text",ExpressionUUID->"5302e6ba-640d-465d-9f54-8bce0dcb5946"]
}, Open  ]],

Cell[CellGroupData[{

Cell["Formulation", "Subsection",ExpressionUUID->"d1954a71-679e-4382-9109-68b56cc6f847"],

Cell[TextData[{
 "Tip: Calculation of kinetic energy in the second form reduces truncation \
error:\n",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     RowBox[{"(", 
      RowBox[{"\[Gamma]", "-", "1"}], ")"}], 
     SubscriptBox["m", "0"], 
     SuperscriptBox["c", "2"]}], "=", 
    FractionBox[
     RowBox[{
      SubscriptBox["m", "0"], "(", 
      RowBox[{"\[Gamma]", " ", "v"}], ")"}], 
     RowBox[{"\[Gamma]", "+", "1"}]]}], TraditionalForm]],
  FormatType->TraditionalForm,ExpressionUUID->
  "158ebecb-5ddb-469d-a087-c39b9188fe87"],
 "."
}], "Text",ExpressionUUID->"40b209e1-acea-4633-8a90-973f1824c2a5"],

Cell[TextData[{
 "The stress-energy tensor:\n",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SuperscriptBox["M", 
     RowBox[{"\[Mu]", "\[VeryThinSpace]", "\[Nu]"}]], "=", 
    RowBox[{"(", GridBox[{
       {
        SuperscriptBox["M", "00"], 
        SuperscriptBox["M", 
         RowBox[{"0", "\[VeryThinSpace]", "i"}]]},
       {
        SuperscriptBox["M", 
         RowBox[{"i", "\[VeryThinSpace]", "0"}]], 
        SuperscriptBox["M", 
         RowBox[{"i", "\[VeryThinSpace]", "j"}]]}
      }], ")"}]}], TraditionalForm]],
  FormatType->TraditionalForm,ExpressionUUID->
  "cc5bc9eb-92c7-481e-b876-3e17722e1463"],
 ",\nwhere\n",
 Cell[BoxData[
  FormBox[
   SuperscriptBox["M", "00"], TraditionalForm]],
  FormatType->TraditionalForm,ExpressionUUID->
  "77f7d24d-26a7-4d47-806b-470989fbb5ec"],
 " is the energy density;\n",
 Cell[BoxData[
  FormBox[
   RowBox[{"c", " ", 
    SuperscriptBox["M", 
     RowBox[{"0", "\[VeryThinSpace]", "i"}]]}], TraditionalForm]],
  FormatType->TraditionalForm,ExpressionUUID->
  "4765f7a5-a8f0-4a0c-9e5a-c814e8dc7a6d"],
 " is the flux of energy in the ",
 Cell[BoxData[
  FormBox["i", TraditionalForm]],
  FormatType->TraditionalForm,ExpressionUUID->
  "ef9c7a0b-2ce6-4cd2-b4fe-9b787c593bdd"],
 "th direction;\n",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SuperscriptBox["M", 
     RowBox[{"i", "\[VeryThinSpace]", "0"}]], "=", 
    SuperscriptBox["M", 
     RowBox[{"0", "\[VeryThinSpace]", "i"}]]}], TraditionalForm]],
  FormatType->TraditionalForm,ExpressionUUID->
  "0d8d1215-ea0d-45cd-8fc9-a8c0502d3c67"],
 "; and\n",
 Cell[BoxData[
  FormBox[
   SuperscriptBox["M", 
    RowBox[{"i", "\[VeryThinSpace]", "j"}]], TraditionalForm]],
  FormatType->TraditionalForm,ExpressionUUID->
  "d8ed43e2-459e-42fe-8b2c-a688f1cc024c"],
 " is the flux of the ",
 Cell[BoxData[
  FormBox["i", TraditionalForm]],
  FormatType->TraditionalForm,ExpressionUUID->
  "9c583351-f7a4-42e5-a586-5d5dd967940b"],
 "th component of momentum in the ",
 Cell[BoxData[
  FormBox["j", TraditionalForm]],
  FormatType->TraditionalForm,ExpressionUUID->
  "d8584de6-a565-4179-be5f-ad2b27f29f35"],
 "th direction.\nIn addition, ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SuperscriptBox["M", 
     RowBox[{"i", "\[VeryThinSpace]", "0"}]], "/", "c"}], TraditionalForm]],
  FormatType->TraditionalForm,ExpressionUUID->
  "e32bdc5a-3350-41d2-a654-9ec6f56db5a9"],
 " is the ",
 Cell[BoxData[
  FormBox["i", TraditionalForm]],
  FormatType->TraditionalForm,ExpressionUUID->
  "a6635da9-0159-4d0e-aa4c-6f70a9682e28"],
 "th component of momentum density, which is equal to\nthe flux of energy in \
the ",
 Cell[BoxData[
  FormBox["i", TraditionalForm]],
  FormatType->TraditionalForm,ExpressionUUID->
  "47e27faa-a47c-40dc-bc68-e4ddc5196c1f"],
 "th direction divided by ",
 Cell[BoxData[
  FormBox[
   SuperscriptBox["c", "2"], TraditionalForm]],
  FormatType->TraditionalForm,ExpressionUUID->
  "0730c38e-78dc-441f-b3b6-02cfd9105809"],
 "."
}], "Text",ExpressionUUID->"3d45c030-8d82-46a2-ab91-c4b0f4f7ab5a"],

Cell[TextData[{
 StyleBox["Reference",
  FontSlant->"Italic"],
 ": Foundations of radiation hydrodynamics, Mihalas, 1984"
}], "Text",ExpressionUUID->"6636fca3-4745-4dfa-8683-05530e397e51"],

Cell[TextData[{
 "Suppose a fluid is composed of a large number of particles, each having a \
rest mass ",
 Cell[BoxData[
  FormBox[
   SubscriptBox["m", "0"], TraditionalForm]],
  FormatType->TraditionalForm,ExpressionUUID->
  "7dacb3a1-0d00-436d-9a37-5d05bad74aa2"],
 ".\nLet ",
 Cell[BoxData[
  FormBox[
   RowBox[{"f", "(", 
    RowBox[{"\[DoubleStruckX]", ",", "\[DoubleStruckP]", ",", "t"}], ")"}], 
   TraditionalForm]],
  FormatType->TraditionalForm,ExpressionUUID->
  "c74d09e0-0f47-4b83-a237-33d87738bf1f"],
 " be the particle distribution function defined such that at time ",
 Cell[BoxData[
  FormBox["t", TraditionalForm]],
  FormatType->TraditionalForm,ExpressionUUID->
  "d976bab7-99a8-4c33-b2ab-fda228d99865"],
 ",\nthe number of particles in a volume element ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"\[DifferentialD]", "\[DoubleStruckX]"}], 
    RowBox[{"\[DifferentialD]", "\[DoubleStruckP]"}]}], TraditionalForm]],
  FormatType->TraditionalForm,ExpressionUUID->
  "f6d9133a-0bd5-4443-95c7-32d0d3303e24"],
 " is ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"f", "(", 
     RowBox[{"\[DoubleStruckX]", ",", "\[DoubleStruckP]", ",", "t"}], ")"}], 
    RowBox[{"\[DifferentialD]", "\[DoubleStruckX]"}], 
    RowBox[{"\[DifferentialD]", "\[DoubleStruckP]"}]}], TraditionalForm]],
  FormatType->TraditionalForm,ExpressionUUID->
  "ad2607bf-8c79-4e8d-ab9d-5a7e4ec0b4e7"],
 "; here all quantities\nare measured in the laboratory frame.\nIf a \
particle's velocity in the lab frame is \[DoubleStruckV], decompose it into\n",
 Cell[BoxData[
  FormBox[
   RowBox[{"\[DoubleStruckV]", "=", 
    RowBox[{"\[DoubleStruckCapitalV]", "+", 
     OverscriptBox["\[Theta]", "\[RightVector]"]}]}], TraditionalForm]],
  FormatType->TraditionalForm,ExpressionUUID->
  "9300595d-adc6-4d35-8b13-a4c8174f812e"],
 ",\nwhere\n",
 Cell[BoxData[
  FormBox[
   RowBox[{"\[DoubleStruckCapitalV]", "=", 
    RowBox[{"\[Integral]", 
     RowBox[{"\[DoubleStruckV]", " ", 
      RowBox[{"f", "(", "\[DoubleStruckP]", ")"}], 
      RowBox[{
       RowBox[{"\[DifferentialD]", "\[DoubleStruckP]"}], "/", 
       RowBox[{"\[Integral]", 
        RowBox[{
         RowBox[{"f", "(", "\[DoubleStruckP]", ")"}], 
         RowBox[{"\[DifferentialD]", "\[DoubleStruckP]"}]}]}]}]}]}]}], 
   TraditionalForm]],
  FormatType->TraditionalForm,ExpressionUUID->
  "a1b3de60-8617-433e-8101-19a56c65d6f1"],
 " \nis the flow velocity of the fluid, and ",
 Cell[BoxData[
  FormBox[
   OverscriptBox["\[Theta]", "\[RightVector]"], TraditionalForm]],
  FormatType->TraditionalForm,ExpressionUUID->
  "5c9de08e-3053-446c-ae5f-d862d90431e7"],
 " is particle's random velocity (measured in the lab frame)\nrelative to the \
flow velocity.\nThe frame moving with the flow velocity is the comoving \
frame; quantities measured in this\nframe are denoted with subscript 0. \nThe \
third set of frames of interest comprises the rest frames of the particles, \
each of which \nmoves with one of the particles. Quantities measured in one \
of these frames will be denoted\nwith a prime, except for a particle's rest \
mass, which will still call ",
 Cell[BoxData[
  FormBox[
   SubscriptBox["m", "0"], TraditionalForm]],
  FormatType->TraditionalForm,ExpressionUUID->
  "eb044d59-33ba-4f8f-b3cf-819c79aff761"],
 "."
}], "Text",ExpressionUUID->"c261bacd-d8bc-4156-9e4f-a9f655d482a3"],

Cell[CellGroupData[{

Cell["Invariance of the distribution function", "Subsubsection",ExpressionUUID->"ac514262-0087-4887-935f-5d35e9b74b49"],

Cell[TextData[{
 "Considering a definite group of particles, observers in both the lab and \
comoving frames will\ncount the same number of particles in the group so that\
\n",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     RowBox[{"f", "(", "\[DoubleStruckP]", ")"}], 
     RowBox[{"\[DifferentialD]", "\[DoubleStruckX]"}], 
     RowBox[{"\[DifferentialD]", "\[DoubleStruckP]"}]}], "=", 
    RowBox[{
     RowBox[{
      SubscriptBox["f", "0"], "(", 
      SubscriptBox["\[DoubleStruckP]", "0"], ")"}], 
     RowBox[{"\[DifferentialD]", 
      SubscriptBox["\[DoubleStruckX]", "0"]}], 
     RowBox[{"\[DifferentialD]", 
      SubscriptBox["\[DoubleStruckP]", "0"]}]}]}], TraditionalForm]],
  FormatType->TraditionalForm,ExpressionUUID->
  "5ddedf54-89fd-44f7-9601-a82cb2eddd3f"],
 ".\nConsider a proper volume element ",
 Cell[BoxData[
  FormBox[
   RowBox[{"\[DifferentialD]", 
    RowBox[{"\[DoubleStruckX]", "'"}]}], TraditionalForm]],
  FormatType->TraditionalForm,ExpressionUUID->
  "0ce354b1-b29c-41a7-ac8f-eeb4cedc811d"],
 " in the rest frame of some particle.\nDue to length contraction an observer \
in the comoving frame will measure its volume to be\n",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"\[DifferentialD]", 
     SubscriptBox["\[DoubleStruckX]", "0"]}], "=", 
    RowBox[{
     SuperscriptBox[
      RowBox[{"(", 
       RowBox[{"1", "-", 
        RowBox[{
         SubsuperscriptBox["\[Theta]", "0", "2"], "/", 
         SuperscriptBox["c", "2"]}]}], ")"}], 
      RowBox[{"1", "/", "2"}]], 
     RowBox[{"\[DifferentialD]", 
      RowBox[{"\[DoubleStruckX]", "'"}]}]}]}], TraditionalForm]],
  FormatType->TraditionalForm,ExpressionUUID->
  "270b642e-c257-4ec0-b321-49b36d5b547f"],
 ",\nwhile an observer in the lab frame will measure its volume as\n",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"\[DifferentialD]", "\[DoubleStruckX]"}], "=", 
    RowBox[{
     SuperscriptBox[
      RowBox[{"(", 
       RowBox[{"1", "-", 
        RowBox[{
         SuperscriptBox["v", "2"], "/", 
         SuperscriptBox["c", "2"]}]}], ")"}], 
      RowBox[{"1", "/", "2"}]], 
     RowBox[{"\[DifferentialD]", 
      RowBox[{"\[DoubleStruckX]", "'"}]}]}]}], TraditionalForm]],
  FormatType->TraditionalForm,ExpressionUUID->
  "b5918a94-336f-467b-af48-6698c7008764"],
 ".\nHence\n",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"\[DifferentialD]", 
     SubscriptBox["\[DoubleStruckX]", "0"]}], "=", 
    RowBox[{
     SuperscriptBox[
      RowBox[{"(", 
       FractionBox[
        RowBox[{"1", "-", 
         RowBox[{
          SubsuperscriptBox["\[Theta]", "0", "2"], "/", 
          SuperscriptBox["c", "2"]}]}], 
        RowBox[{"1", "-", 
         RowBox[{
          SuperscriptBox["v", "2"], "/", 
          SuperscriptBox["c", "2"]}]}]], ")"}], 
      RowBox[{"1", "/", "2"}]], 
     RowBox[{"\[DifferentialD]", "\[DoubleStruckX]"}]}]}], TraditionalForm]],
  FormatType->TraditionalForm,ExpressionUUID->
  "2419ba0a-4ef4-4019-af9c-09a1160ee89a"],
 ".\nOn the other hand, the energy in the comoving frame of a particle rest \
in the proper volume is\n",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubscriptBox["\[CurlyEpsilon]", "0"], "=", 
    RowBox[{
     SubscriptBox["m", "0"], 
     RowBox[{
      SuperscriptBox["c", "2"], "/", 
      SuperscriptBox[
       RowBox[{"(", 
        RowBox[{"1", "-", 
         RowBox[{
          SubsuperscriptBox["\[Theta]", "0", "2"], "/", 
          SuperscriptBox["c", "2"]}]}], ")"}], 
       RowBox[{"1", "/", "2"}]]}]}]}], TraditionalForm]],
  FormatType->TraditionalForm,ExpressionUUID->
  "de6529ea-2e67-46ce-b16c-8774402dddfc"],
 ",\nand the energy in the lab frame is\n",
 Cell[BoxData[
  FormBox[
   RowBox[{"\[CurlyEpsilon]", "=", 
    RowBox[{
     SubscriptBox["m", "0"], 
     RowBox[{
      SuperscriptBox["c", "2"], "/", 
      SuperscriptBox[
       RowBox[{"(", 
        RowBox[{"1", "-", 
         RowBox[{
          SuperscriptBox["v", "2"], "/", 
          SuperscriptBox["c", "2"]}]}], ")"}], 
       RowBox[{"1", "/", "2"}]]}]}]}], TraditionalForm]],
  FormatType->TraditionalForm,ExpressionUUID->
  "8e394b2e-9228-46fe-9921-182627a95a09"],
 ".\nTherefore,\n",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"\[DifferentialD]", 
     SubscriptBox["\[DoubleStruckX]", "0"]}], "=", 
    RowBox[{
     RowBox[{"(", 
      RowBox[{"\[CurlyEpsilon]", "/", 
       SubscriptBox["\[CurlyEpsilon]", "0"]}], ")"}], 
     RowBox[{"\[DifferentialD]", "\[DoubleStruckX]"}]}]}], TraditionalForm]],
  FormatType->TraditionalForm,ExpressionUUID->
  "09ac942e-1bd8-4d13-aa7e-893ee8b53f2f"],
 ".\n\nWe can relate ",
 Cell[BoxData[
  FormBox[
   RowBox[{"\[DifferentialD]", "\[DoubleStruckP]"}], TraditionalForm]],
  FormatType->TraditionalForm,ExpressionUUID->
  "454b4ae0-8526-4b5b-b5bc-07f7c3b8b554"],
 " to ",
 Cell[BoxData[
  FormBox[
   RowBox[{"\[DifferentialD]", 
    SubscriptBox["\[DoubleStruckP]", "0"]}], TraditionalForm]],
  FormatType->TraditionalForm,ExpressionUUID->
  "a7ad428d-6fbe-4c28-aa94-09692a217c49"],
 " by applying the general Lorentz transformation to particle's four-\n\
momentum measured in the comoving frame. We get\n",
 Cell[BoxData[
  FormBox[
   RowBox[{"\[CurlyEpsilon]", "=", 
    RowBox[{"\[Gamma]", " ", 
     RowBox[{"(", 
      RowBox[{
       SubscriptBox["\[CurlyEpsilon]", "0"], "+", 
       RowBox[{"\[DoubleStruckCapitalV]", "\[CenterDot]", 
        SubscriptBox["\[DoubleStruckP]", "0"]}]}], ")"}]}]}], 
   TraditionalForm]],
  FormatType->TraditionalForm,ExpressionUUID->
  "613ad886-bc10-4408-8a40-24757d79ee62"],
 ";\n",
 Cell[BoxData[
  FormBox[
   RowBox[{"\[DoubleStruckP]", "=", 
    RowBox[{
     SubscriptBox["\[DoubleStruckP]", "0"], "+", 
     RowBox[{"\[DoubleStruckCapitalV]", " ", 
      RowBox[{"(", 
       RowBox[{
        RowBox[{"\[Gamma]", " ", 
         RowBox[{"(", 
          RowBox[{
           SubscriptBox["\[CurlyEpsilon]", "0"], "/", 
           SuperscriptBox["c", "2"]}], ")"}]}], "+", 
        RowBox[{
         RowBox[{"(", 
          RowBox[{"\[Gamma]", "-", "1"}], ")"}], 
         RowBox[{"\[DoubleStruckCapitalV]", "\[CenterDot]", 
          RowBox[{
           SubscriptBox["\[DoubleStruckP]", "0"], "/", 
           SuperscriptBox["V", "2"]}]}]}]}], ")"}]}]}]}], TraditionalForm]],
  FormatType->TraditionalForm,ExpressionUUID->
  "3bb68b9f-cc7c-4869-8132-e315c2016ec6"],
 ",\nwhere ",
 Cell[BoxData[
  FormBox[
   RowBox[{"\[Gamma]", "=", 
    SuperscriptBox[
     RowBox[{"(", 
      RowBox[{"1", "-", 
       RowBox[{
        SuperscriptBox["V", "2"], "/", 
        SuperscriptBox["c", "2"]}]}], ")"}], 
     RowBox[{
      RowBox[{"-", "1"}], "/", "2"}]]}], TraditionalForm]],
  FormatType->TraditionalForm,ExpressionUUID->
  "c0477c28-deef-4513-a753-c2fec05dbfcf"],
 ".\nAfter some length calculation, we have\n",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"\[DifferentialD]", "\[DoubleStruckP]"}], "=", 
    RowBox[{
     RowBox[{
      RowBox[{"(", 
       RowBox[{"\[CurlyEpsilon]", "/", 
        SubscriptBox["\[CurlyEpsilon]", "0"]}], ")"}], 
      RowBox[{"\[DifferentialD]", 
       SubscriptBox["\[DoubleStruckP]", "0"]}]}], "=", 
     RowBox[{"\[Gamma]", " ", 
      RowBox[{"(", 
       RowBox[{"1", "/", 
        SubscriptBox["\[CurlyEpsilon]", "0"]}], ")"}], 
      RowBox[{"(", 
       RowBox[{
        SubscriptBox["\[CurlyEpsilon]", "0"], "+", 
        RowBox[{"\[DoubleStruckCapitalV]", "\[CenterDot]", 
         SubscriptBox["\[DoubleStruckP]", "0"]}]}], ")"}], 
      RowBox[{"\[DifferentialD]", 
       SubscriptBox["\[DoubleStruckP]", "0"]}]}]}]}], TraditionalForm]],
  FormatType->TraditionalForm,ExpressionUUID->
  "13711d44-2caa-4db7-8adf-9117c032782c"],
 ".\nHence ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     RowBox[{"\[DifferentialD]", "\[DoubleStruckX]"}], 
     RowBox[{"\[DifferentialD]", "\[DoubleStruckP]"}]}], "=", 
    RowBox[{
     RowBox[{"\[DifferentialD]", 
      SubscriptBox["\[DoubleStruckX]", "0"]}], 
     RowBox[{"\[DifferentialD]", 
      SubscriptBox["\[DoubleStruckP]", "0"]}]}]}], TraditionalForm]],
  FormatType->TraditionalForm,ExpressionUUID->
  "d7726aec-6403-45b2-8239-14ee708d4973"],
 ", and so the distribution function ",
 Cell[BoxData[
  FormBox["f", TraditionalForm]],
  FormatType->TraditionalForm,ExpressionUUID->
  "cc812e0a-efa9-49c4-bc3f-ab62dd80cca4"],
 " is invariant under Lorentz transformation,\n",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"f", "(", "\[DoubleStruckP]", ")"}], "=", 
    RowBox[{
     SubscriptBox["f", "0"], "(", 
     SubscriptBox["\[DoubleStruckP]", "0"], ")"}]}], TraditionalForm]],
  FormatType->TraditionalForm,ExpressionUUID->
  "83c19aa4-e78c-443d-adad-354e23b9fe7d"],
 "."
}], "Text",ExpressionUUID->"b0d22764-5d84-4deb-9ec5-d63c338da13d"]
}, Open  ]],

Cell[CellGroupData[{

Cell["Comoving-frame number density, energy density, and pressure", \
"Subsubsection",ExpressionUUID->"b6f2595f-21f6-4b20-91fa-270853cb7396"],

Cell[TextData[{
 "Comoving-frame particle number density:\n",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubscriptBox["n", "0"], "=", 
    RowBox[{"\[Integral]", 
     RowBox[{
      RowBox[{
       SubscriptBox["f", "0"], "(", 
       SubscriptBox["\[DoubleStruckP]", "0"], ")"}], 
      RowBox[{"\[DifferentialD]", 
       SubscriptBox["\[DoubleStruckP]", "0"]}]}]}]}], TraditionalForm]],
  ExpressionUUID->"56a35255-cbbf-4dd2-9e10-c72f0611f018"],
 ".\nThe density of rest mass in the comoving frame is ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubscriptBox["\[Rho]", "0"], "=", 
    RowBox[{
     SubscriptBox["m", "0"], 
     SubscriptBox["n", "0"]}]}], TraditionalForm]],ExpressionUUID->
  "f1a9bbcd-a6f7-4b86-9f29-0142f47c2eea"],
 ".\nThe total energy density in the comoving frame:\n",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     SubscriptBox["\[Rho]", "00"], 
     SuperscriptBox["c", "2"]}], "=", 
    RowBox[{
     RowBox[{"\[Integral]", 
      RowBox[{
       SubscriptBox["m", "0"], 
       RowBox[{
        SuperscriptBox["c", "2"], "/", 
        SuperscriptBox[
         RowBox[{"(", 
          RowBox[{"1", "-", 
           RowBox[{
            SubsuperscriptBox["\[Theta]", "0", "2"], "/", 
            SuperscriptBox["c", "2"]}]}], ")"}], 
         RowBox[{"1", "/", "2"}]]}], 
       RowBox[{
        SubscriptBox["f", "0"], "(", 
        SubscriptBox["\[DoubleStruckP]", "0"], ")"}], 
       RowBox[{"\[DifferentialD]", 
        SubscriptBox["\[DoubleStruckP]", "0"]}]}]}], "=", 
     RowBox[{"\[Integral]", 
      RowBox[{
       SubscriptBox["\[CurlyEpsilon]", "0"], 
       RowBox[{
        SubscriptBox["f", "0"], "(", 
        SubscriptBox["\[DoubleStruckP]", "0"], ")"}], 
       RowBox[{"\[DifferentialD]", 
        SubscriptBox["\[DoubleStruckP]", "0"]}]}]}]}]}], TraditionalForm]],
  ExpressionUUID->"9998b3e7-1199-4441-b54b-fe20f51c89bc"],
 ",\nwhere ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubscriptBox["\[CurlyEpsilon]", "0"], "=", 
    RowBox[{
     SubscriptBox["m", "0"], 
     RowBox[{
      SuperscriptBox["c", "2"], "/", 
      SuperscriptBox[
       RowBox[{"(", 
        RowBox[{"1", "-", 
         RowBox[{
          SubsuperscriptBox["\[Theta]", "0", "2"], "/", 
          SuperscriptBox["c", "2"]}]}], ")"}], 
       RowBox[{"1", "/", "2"}]]}]}]}], TraditionalForm]],ExpressionUUID->
  "bb26d883-43f6-4bc9-973b-c1760f8286b1"],
 ". In relativistic fluid formulation,\n",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubscriptBox["\[Rho]", "00"], "=", 
    RowBox[{
     SubscriptBox["\[Rho]", "0"], "(", 
     RowBox[{"1", "+", 
      RowBox[{"e", "/", 
       SuperscriptBox["c", "2"]}]}], ")"}]}], TraditionalForm]],
  ExpressionUUID->"32bca157-8c47-4fa5-ae68-c29c17653b2e"],
 ",\nwhere ",
 Cell[BoxData[
  FormBox["e", TraditionalForm]],ExpressionUUID->
  "a7e8672f-6e48-4f05-9d9f-641f45862fd1"],
 " is the specific internal energy of the gas produced by the microscopic \
motions\nof its constituent particles. ",
 Cell[BoxData[
  FormBox[
   SubscriptBox["\[Rho]", "00"], TraditionalForm]],ExpressionUUID->
  "97b95460-899b-4258-8580-ce48e9f797b1"],
 " is the total mass density of the fluid, including the mass\nequivalent of \
its thermal energy, and ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubscriptBox["\[Rho]", "00"], 
    SuperscriptBox["c", "2"]}], TraditionalForm]],ExpressionUUID->
  "7695663b-0b99-4965-bc1a-0043ae92de6f"],
 " is the total energy density of the fluid.\nThe momentum flux (pressure) \
tensor in the comoving frame is \n",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubsuperscriptBox["P", "0", 
     RowBox[{"i", "\[VeryThinSpace]", "j"}]], "=", 
    RowBox[{"\[Integral]", 
     RowBox[{
      SubsuperscriptBox["\[Theta]", "0", "i"], 
      SubsuperscriptBox["p", "0", "j"], 
      RowBox[{
       SubscriptBox["f", "0"], "(", 
       SubscriptBox["\[DoubleStruckP]", "0"], ")"}], 
      RowBox[{"\[DifferentialD]", 
       SubscriptBox["\[DoubleStruckP]", "0"]}]}]}]}], TraditionalForm]],
  ExpressionUUID->"9c866d72-1a8a-4133-beb2-31941f770997"],
 ".\nFor an isotropic distribution function in the comoving frame, the \
pressure tensor is \n",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubsuperscriptBox["P", "0", 
     RowBox[{"i", "\[VeryThinSpace]", "j"}]], "=", 
    RowBox[{"P", " ", 
     SuperscriptBox["\[Delta]", 
      RowBox[{"i", "\[VeryThinSpace]", "j"}]]}]}], TraditionalForm]],
  ExpressionUUID->"9ef332a9-692b-4056-b99a-e51d772f97b6"],
 ",\nwhere the pressure is given by \n",
 Cell[BoxData[
  FormBox[
   RowBox[{"P", "=", 
    RowBox[{"\[Integral]", 
     RowBox[{
      SubsuperscriptBox["\[Theta]", "0", "i"], 
      SubsuperscriptBox["p", "0", "i"], 
      RowBox[{
       SubscriptBox["f", "0"], "(", 
       SubscriptBox["\[DoubleStruckP]", "0"], ")"}], 
      RowBox[{"\[DifferentialD]", 
       SubscriptBox["\[DoubleStruckP]", "0"]}]}]}]}], TraditionalForm]],
  ExpressionUUID->"3b55ebae-3d9a-4866-9f9e-5e732a17d564"],
 " (no sum over ",
 Cell[BoxData[
  FormBox["i", TraditionalForm]],ExpressionUUID->
  "6de7d79a-fcb7-412c-9c01-644785581597"],
 ").\nThe stress-energy tensor in the comoving frame (assuming an isotropic \
distribution) is\n",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubsuperscriptBox["M", "0", 
     RowBox[{"\[Mu]", "\[VeryThinSpace]", "\[Nu]"}]], "=", 
    RowBox[{"(", GridBox[{
       {
        RowBox[{
         SubscriptBox["\[Rho]", "00"], 
         SuperscriptBox["c", "2"]}], "0", "0", "0"},
       {"0", "P", "0", "0"},
       {"0", "0", "P", "0"},
       {"0", "0", "0", "P"}
      }], ")"}]}], TraditionalForm]],ExpressionUUID->
  "8330df3d-6fb4-403c-8576-d613ddbb07ec"],
 ".\nThe space components ",
 Cell[BoxData[
  FormBox[
   SuperscriptBox["M", 
    RowBox[{"\[Mu]", "\[VeryThinSpace]", "\[Nu]"}]], TraditionalForm]],
  ExpressionUUID->"3ce61617-354c-471b-baf1-098dc3ff01a3"],
 " of the stress-energy tensor account for the momentum flux \ndensity, both \
macroscopic and microscopic (i.e., pressure)."
}], "Text",ExpressionUUID->"18a45b87-e869-4b6d-9c63-a9e67ae068bd"]
}, Open  ]],

Cell[CellGroupData[{

Cell["\<\
Lab-frame particle density, energy density, and momentum density\
\>", "Subsubsection",ExpressionUUID->"ea8636f8-ae11-4da6-8b84-58371e3ec1a8"],

Cell[TextData[{
 "The particle density in the lab frame:\n",
 Cell[BoxData[{
  FormBox[
   RowBox[{"n", "=", 
    RowBox[{
     RowBox[{"\[Integral]", 
      RowBox[{
       RowBox[{"f", "(", "\[DoubleStruckP]", ")"}], 
       RowBox[{"\[DifferentialD]", "\[DoubleStruckP]"}]}]}], "=", 
     RowBox[{"\[Integral]", 
      RowBox[{"\[Gamma]", " ", 
       RowBox[{"(", 
        RowBox[{"1", "/", 
         SubscriptBox["\[CurlyEpsilon]", "0"]}], ")"}], 
       RowBox[{"(", 
        RowBox[{
         SubscriptBox["\[CurlyEpsilon]", "0"], "+", 
         RowBox[{"\[DoubleStruckCapitalV]", "\[CenterDot]", 
          SubscriptBox["\[DoubleStruckP]", "0"]}]}], ")"}], 
       RowBox[{
        SubscriptBox["f", "0"], "(", 
        SubscriptBox["\[DoubleStruckP]", "0"], ")"}], 
       RowBox[{"\[DifferentialD]", 
        SubscriptBox["\[DoubleStruckP]", "0"]}]}]}]}]}], 
   TraditionalForm], "\[IndentingNewLine]", 
  FormBox[
   RowBox[{"=", 
    RowBox[{"\[Gamma]", 
     RowBox[{"\[Integral]", 
      RowBox[{
       RowBox[{"(", 
        RowBox[{"1", "+", 
         RowBox[{"\[DoubleStruckCapitalV]", "\[CenterDot]", 
          RowBox[{
           SubscriptBox["\[DoubleStruckP]", "0"], "/", 
           SubscriptBox["\[CurlyEpsilon]", "0"]}]}]}], ")"}], 
       RowBox[{
        SubscriptBox["f", "0"], "(", 
        SubscriptBox["\[DoubleStruckP]", "0"], ")"}], 
       RowBox[{"\[DifferentialD]", 
        SubscriptBox["\[DoubleStruckP]", "0"]}]}]}]}]}], TraditionalForm]}],
  ExpressionUUID->"dd6c8295-4359-4001-a2f2-abbdb889623b"],
 ",\nwhere ",
 Cell[BoxData[
  FormBox[
   RowBox[{"\[Gamma]", "=", 
    SuperscriptBox[
     RowBox[{"(", 
      RowBox[{"1", "-", 
       RowBox[{
        SuperscriptBox["V", "2"], "/", 
        SuperscriptBox["c", "2"]}]}], ")"}], 
     RowBox[{
      RowBox[{"-", "1"}], "/", "2"}]]}], TraditionalForm]],ExpressionUUID->
  "9c3872cb-0eb0-4749-848e-3662074eaf2b"],
 ".\nIf ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubscriptBox["f", "0"], "(", 
    SubscriptBox["\[DoubleStruckP]", "0"], ")"}], TraditionalForm]],
  ExpressionUUID->"450f7e60-eb4e-42c6-b7e2-a3a9b035b24a"],
 " is symmetric about ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    FractionBox["\[DoubleStruckCapitalV]", "V"], "\[CenterDot]", 
    SubscriptBox["\[DoubleStruckP]", "0"]}], TraditionalForm]],ExpressionUUID->
  "93e7f270-20aa-41c3-affc-192f2169e397"],
 ", then the second term in the integral vanishes.\nHence\n",
 Cell[BoxData[
  FormBox[
   RowBox[{"n", "=", 
    RowBox[{
     RowBox[{"\[Gamma]", 
      RowBox[{"\[Integral]", 
       RowBox[{
        RowBox[{
         SubscriptBox["f", "0"], "(", 
         SubscriptBox["\[DoubleStruckP]", "0"], ")"}], 
        RowBox[{"\[DifferentialD]", 
         SubscriptBox["\[DoubleStruckP]", "0"]}]}]}]}], "=", 
     RowBox[{"\[Gamma]", " ", 
      SubscriptBox["n", "0"]}]}]}], TraditionalForm]],ExpressionUUID->
  "db198b40-db85-4022-8546-97746cfc81ec"],
 ".\nThe energy density in the lab frame:\n",
 Cell[BoxData[{
  FormBox[
   RowBox[{"ED", "=", 
    RowBox[{
     RowBox[{"\[Integral]", 
      RowBox[{"\[CurlyEpsilon]", " ", 
       RowBox[{"f", "(", "\[DoubleStruckP]", ")"}], 
       RowBox[{"\[DifferentialD]", "\[DoubleStruckP]"}]}]}], "=", 
     RowBox[{"\[Integral]", 
      RowBox[{"\[Gamma]", " ", 
       RowBox[{"(", 
        RowBox[{
         SubscriptBox["\[CurlyEpsilon]", "0"], "+", 
         RowBox[{"\[DoubleStruckCapitalV]", "\[CenterDot]", 
          SubscriptBox["\[DoubleStruckP]", "0"]}]}], ")"}], 
       RowBox[{
        SubscriptBox["f", "0"], "(", 
        SubscriptBox["\[DoubleStruckP]", "0"], ")"}], 
       RowBox[{"(", 
        RowBox[{"\[CurlyEpsilon]", "/", 
         SubscriptBox["\[CurlyEpsilon]", "0"]}], ")"}], 
       RowBox[{"\[DifferentialD]", 
        SubscriptBox["\[DoubleStruckP]", "0"]}]}]}]}]}], 
   TraditionalForm], "\[IndentingNewLine]", 
  FormBox[
   RowBox[{"=", 
    RowBox[{
     SuperscriptBox["\[Gamma]", "2"], 
     RowBox[{"\[Integral]", 
      RowBox[{
       RowBox[{"(", 
        RowBox[{"1", "/", 
         SubscriptBox["\[CurlyEpsilon]", "0"]}], ")"}], 
       SuperscriptBox[
        RowBox[{"(", 
         RowBox[{
          SubscriptBox["\[CurlyEpsilon]", "0"], "+", 
          RowBox[{"\[DoubleStruckCapitalV]", "\[CenterDot]", 
           SubscriptBox["\[DoubleStruckP]", "0"]}]}], ")"}], "2"], 
       RowBox[{
        SubscriptBox["f", "0"], "(", 
        SubscriptBox["\[DoubleStruckP]", "0"], ")"}], 
       RowBox[{"\[DifferentialD]", 
        SubscriptBox["\[DoubleStruckP]", "0"]}]}]}]}]}], 
   TraditionalForm], "\[IndentingNewLine]", 
  FormBox[
   RowBox[{"=", 
    RowBox[{
     SuperscriptBox["\[Gamma]", "2"], 
     RowBox[{"\[Integral]", 
      RowBox[{
       RowBox[{"(", 
        RowBox[{"1", "/", 
         SubscriptBox["\[CurlyEpsilon]", "0"]}], ")"}], 
       RowBox[{"(", 
        RowBox[{
         SubsuperscriptBox["\[CurlyEpsilon]", "0", "2"], "+", 
         SuperscriptBox[
          RowBox[{"(", 
           RowBox[{"\[DoubleStruckCapitalV]", "\[CenterDot]", 
            SubscriptBox["\[DoubleStruckP]", "0"]}], ")"}], "2"], "+", 
         RowBox[{"2", 
          SubscriptBox["\[CurlyEpsilon]", "0"], 
          RowBox[{"\[DoubleStruckCapitalV]", "\[CenterDot]", 
           SubscriptBox["\[DoubleStruckP]", "0"]}]}]}], ")"}], 
       RowBox[{
        SubscriptBox["f", "0"], "(", 
        SubscriptBox["\[DoubleStruckP]", "0"], ")"}], 
       RowBox[{"\[DifferentialD]", 
        SubscriptBox["\[DoubleStruckP]", "0"]}]}]}]}]}], 
   TraditionalForm], "\[IndentingNewLine]", 
  FormBox[
   RowBox[{"=", 
    RowBox[{
     SuperscriptBox["\[Gamma]", "2"], 
     RowBox[{"\[Integral]", 
      RowBox[{
       RowBox[{"(", 
        RowBox[{
         SubscriptBox["\[CurlyEpsilon]", "0"], "+", 
         RowBox[{
          SuperscriptBox[
           RowBox[{"(", 
            RowBox[{"\[DoubleStruckCapitalV]", "\[CenterDot]", 
             SubscriptBox["\[DoubleStruckP]", "0"]}], ")"}], "2"], "/", 
          SubscriptBox["\[CurlyEpsilon]", "0"]}]}], ")"}], 
       RowBox[{
        SubscriptBox["f", "0"], "(", 
        SubscriptBox["\[DoubleStruckP]", "0"], ")"}], 
       RowBox[{"\[DifferentialD]", 
        SubscriptBox["\[DoubleStruckP]", "0"]}]}]}]}]}], TraditionalForm]}],
  ExpressionUUID->"3fd3ae72-7c24-4a4e-9a4c-70da87497a7b"],
 ",\nwhere the assumption of symmetric ",
 Cell[BoxData[
  FormBox[
   SubscriptBox["f", "0"], TraditionalForm]],ExpressionUUID->
  "05c2b83e-0946-4009-a4e3-e54f894ced3b"],
 " is made.\nUsing ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubscriptBox["\[DoubleStruckP]", "0"], "=", 
    RowBox[{
     SubscriptBox["\[CurlyEpsilon]", "0"], 
     RowBox[{
      SubscriptBox[
       OverscriptBox["\[Theta]", "\[RightVector]"], "0"], "/", 
      SuperscriptBox["c", "2"]}]}]}], TraditionalForm]],ExpressionUUID->
  "4d1b7a66-4c70-44ec-a922-c6942c2a363c"],
 ",\n",
 Cell[BoxData[{
  FormBox[
   RowBox[{"ED", "=", 
    RowBox[{
     SuperscriptBox["\[Gamma]", "2"], 
     RowBox[{"\[Integral]", 
      RowBox[{
       RowBox[{"(", 
        RowBox[{
         SubscriptBox["\[CurlyEpsilon]", "0"], "+", 
         RowBox[{
          RowBox[{"(", 
           RowBox[{"\[DoubleStruckCapitalV]", "\[CenterDot]", 
            SubscriptBox["\[DoubleStruckP]", "0"]}], ")"}], 
          RowBox[{
           RowBox[{"(", 
            RowBox[{"\[DoubleStruckCapitalV]", "\[CenterDot]", 
             SubscriptBox[
              OverscriptBox["\[Theta]", "\[RightVector]"], "0"]}], ")"}], "/", 
           SuperscriptBox["c", "2"]}]}]}], ")"}], 
       RowBox[{
        SubscriptBox["f", "0"], "(", 
        SubscriptBox["\[DoubleStruckP]", "0"], ")"}], 
       RowBox[{"\[DifferentialD]", 
        SubscriptBox["\[DoubleStruckP]", "0"]}]}]}]}]}], 
   TraditionalForm], "\[IndentingNewLine]", 
  FormBox[
   RowBox[{"=", 
    RowBox[{
     RowBox[{
      SuperscriptBox["\[Gamma]", "2"], 
      RowBox[{"\[Integral]", 
       RowBox[{
        SubscriptBox["\[CurlyEpsilon]", "0"], 
        RowBox[{
         SubscriptBox["f", "0"], "(", 
         SubscriptBox["\[DoubleStruckP]", "0"], ")"}], 
        RowBox[{"\[DifferentialD]", 
         SubscriptBox["\[DoubleStruckP]", "0"]}]}]}]}], "+", 
     RowBox[{
      RowBox[{
       SuperscriptBox["\[Gamma]", "2"], "(", 
       RowBox[{
        SuperscriptBox["V", "2"], "/", 
        SuperscriptBox["c", "2"]}], ")"}], 
      RowBox[{"\[Integral]", 
       RowBox[{
        RowBox[{"(", 
         RowBox[{"\[DoubleStruckCapitalI]", "\[CenterDot]", 
          SubscriptBox["\[DoubleStruckP]", "0"]}], ")"}], 
        RowBox[{"(", 
         RowBox[{"\[DoubleStruckCapitalI]", "\[CenterDot]", 
          SubscriptBox[
           OverscriptBox["\[Theta]", "\[RightVector]"], "0"]}], ")"}], 
        RowBox[{
         SubscriptBox["f", "0"], "(", 
         SubscriptBox["\[DoubleStruckP]", "0"], ")"}], 
        RowBox[{"\[DifferentialD]", 
         SubscriptBox["\[DoubleStruckP]", "0"]}]}]}]}]}]}], 
   TraditionalForm], "\[IndentingNewLine]", 
  FormBox[
   RowBox[{"=", 
    RowBox[{
     SuperscriptBox["\[Gamma]", "2"], "(", 
     RowBox[{
      RowBox[{
       SubscriptBox["\[Rho]", "00"], 
       SuperscriptBox["c", "2"]}], "+", 
      RowBox[{
       RowBox[{"(", 
        RowBox[{
         SuperscriptBox["V", "2"], "/", 
         SuperscriptBox["c", "2"]}], ")"}], 
       RowBox[{"(", 
        RowBox[{"\[DoubleStruckCapitalI]", "\[CenterDot]", 
         SubscriptBox["\[DoubleStruckCapitalP]", "0"], "\[CenterDot]", 
         "\[DoubleStruckCapitalI]"}], ")"}]}]}], ")"}]}], TraditionalForm]}],
  ExpressionUUID->"a4534965-d57b-4898-84f8-792ac594b7cb"],
 ",\nwhere \[DoubleStruckCapitalI] is the unit vector along \
\[DoubleStruckCapitalV] and ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubscriptBox["\[DoubleStruckCapitalP]", "0"], "=", 
    RowBox[{"\[Integral]", 
     RowBox[{
      SubscriptBox["\[DoubleStruckP]", "0"], 
      SubscriptBox[
       OverscriptBox["\[Theta]", "\[RightVector]"], "0"], 
      RowBox[{
       SubscriptBox["f", "0"], "(", 
       SubscriptBox["\[DoubleStruckP]", "0"], ")"}], 
      RowBox[{"\[DifferentialD]", 
       SubscriptBox["\[DoubleStruckP]", "0"]}]}]}]}], TraditionalForm]],
  ExpressionUUID->"8c1c8f78-cc55-4eec-869c-fc018c91c0af"],
 " is the pressure tensor \nin the comoving frame.\nThis is identical to ",
 Cell[BoxData[
  FormBox[
   SuperscriptBox["M", "00"], TraditionalForm]],ExpressionUUID->
  "cc629769-f524-40f5-af19-63776337b17e"],
 " in the stress-energy tensor.\nFor an isotropic distribution,  ",
 Cell[BoxData[
  FormBox[
   RowBox[{"ED", "=", 
    RowBox[{
     SuperscriptBox["\[Gamma]", "2"], "(", 
     RowBox[{
      RowBox[{
       SubscriptBox["\[Rho]", "00"], 
       SuperscriptBox["c", "2"]}], "+", 
      RowBox[{
       RowBox[{
        SuperscriptBox["V", "2"], "/", 
        SuperscriptBox["c", "2"]}], "P"}]}], ")"}]}], TraditionalForm]],
  ExpressionUUID->"2e3d8141-9006-494c-8a0f-94ff749559df"],
 ".\nIf ",
 Cell[BoxData[
  FormBox[
   RowBox[{"V", "\[LessLess]", "c"}], TraditionalForm]],ExpressionUUID->
  "fc0abdc1-1eb8-4346-ba58-d71722925c3d"],
 ", the second term may be neglected, ",
 Cell[BoxData[
  FormBox[
   RowBox[{"ED", "\[TildeTilde]", 
    RowBox[{
     SuperscriptBox["\[Gamma]", "2"], 
     SubscriptBox["\[Rho]", "00"], 
     SuperscriptBox["c", "2"]}]}], TraditionalForm]],ExpressionUUID->
  "98bc78da-50a2-4a33-afcc-d4471d071214"],
 ".\nThe momentum density in the lab frame:\n",
 Cell[BoxData[{
  FormBox[
   RowBox[{"MD", "=", 
    RowBox[{
     RowBox[{"\[Integral]", 
      RowBox[{"\[DoubleStruckP]", " ", 
       RowBox[{"f", "(", "\[DoubleStruckP]", ")"}], 
       RowBox[{"\[DifferentialD]", "\[DoubleStruckP]"}]}]}], "=", 
     RowBox[{"\[Integral]", 
      RowBox[{
       RowBox[{"(", 
        RowBox[{"\[CurlyEpsilon]", "/", 
         SubscriptBox["\[CurlyEpsilon]", "0"]}], ")"}], "\[DoubleStruckP]", 
       " ", 
       RowBox[{
        SubscriptBox["f", "0"], "(", 
        SubscriptBox["\[DoubleStruckP]", "0"], ")"}], 
       RowBox[{"\[DifferentialD]", 
        SubscriptBox["\[DoubleStruckP]", "0"]}]}]}]}]}], 
   TraditionalForm], "\[IndentingNewLine]", 
  FormBox[
   RowBox[{"=", 
    RowBox[{"\[Gamma]", 
     RowBox[{"\[Integral]", 
      RowBox[{
       RowBox[{"(", 
        RowBox[{"1", "+", 
         RowBox[{"\[DoubleStruckCapitalV]", "\[CenterDot]", 
          RowBox[{
           SubscriptBox["\[DoubleStruckP]", "0"], "/", 
           SubscriptBox["\[CurlyEpsilon]", "0"]}]}]}], ")"}], 
       RowBox[{"(", 
        RowBox[{
         SubscriptBox["\[DoubleStruckP]", "0"], "+", 
         RowBox[{
          RowBox[{"(", 
           RowBox[{"\[Gamma]", "-", "1"}], ")"}], 
          RowBox[{
           RowBox[{"(", 
            RowBox[{"\[DoubleStruckCapitalV]", "\[CenterDot]", 
             SubscriptBox["\[DoubleStruckP]", "0"]}], ")"}], "/", 
