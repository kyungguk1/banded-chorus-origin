  FormatType->TraditionalForm,ExpressionUUID->
  "47f27575-fd08-4448-a7ac-b41ca95abaca"],
 ", one easily find\n",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     SubscriptBox["\[Rho]", "00"], 
     SuperscriptBox["c", "2"]}], "=", 
    RowBox[{
     RowBox[{
      RowBox[{
       SubscriptBox["\[Rho]", "0"], 
       SuperscriptBox["c", "2"]}], "+", 
      RowBox[{
       FractionBox["1", "2"], 
       RowBox[{
        SubscriptBox["n", "0"], "(", 
        RowBox[{
         SubscriptBox["T", "\[DoubleVerticalBar]"], "+", 
         RowBox[{"2", 
          SubscriptBox["T", "\[UpTee]"]}]}], ")"}]}]}], "=", 
     RowBox[{
      RowBox[{
       SubscriptBox["\[Rho]", "0"], 
       SuperscriptBox["c", "2"]}], "+", 
      RowBox[{
       FractionBox["1", "2"], 
       RowBox[{"(", 
        RowBox[{
         SubscriptBox["P", "\[DoubleVerticalBar]"], "+", 
