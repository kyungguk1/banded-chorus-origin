        RowBox[{"\[DifferentialD]", 
         SubscriptBox["\[DoubleStruckP]", "0"]}]}]}]}], "+", 
     RowBox[{
      RowBox[{
       SuperscriptBox["\[Gamma]", "2"], "(", 
       RowBox[{
        SuperscriptBox["V", "2"], "/", 
        SuperscriptBox["c", "2"]}], ")"}], 
      RowBox[{"\[Integral]", 
       RowBox[{
        RowBox[{"(", 
         RowBox[{"\[DoubleStruckCapitalI]", "\[CenterDot]", 
          SubscriptBox["\[DoubleStruckP]", "0"]}], ")"}], 
        RowBox[{"(", 
         RowBox[{"\[DoubleStruckCapitalI]", "\[CenterDot]", 
          SubscriptBox[
           OverscriptBox["\[Theta]", "\[RightVector]"], "0"]}], ")"}], 
        RowBox[{
         SubscriptBox["f", "0"], "(", 
         SubscriptBox["\[DoubleStruckP]", "0"], ")"}], 
        RowBox[{"\[DifferentialD]", 
         SubscriptBox["\[DoubleStruckP]", "0"]}]}]}]}]}]}], 
   TraditionalForm], "\[IndentingNewLine]", 
  FormBox[
   RowBox[{"=", 
    RowBox[{
     SuperscriptBox["\[Gamma]", "2"], "(", 
     RowBox[{
      RowBox[{
       SubscriptBox["\[Rho]", "00"], 
