          RowBox[{"(", 
           RowBox[{"\[Gamma]", "-", "1"}], ")"}], 
          RowBox[{
           RowBox[{"(", 
            RowBox[{"\[DoubleStruckCapitalV]", "\[CenterDot]", 
             SubscriptBox["\[DoubleStruckP]", "0"]}], ")"}], "/", 
           SuperscriptBox["V", "2"]}], "\[DoubleStruckCapitalV]"}], "+", 
         RowBox[{
          RowBox[{"(", 
           RowBox[{"\[Gamma]", " ", 
            RowBox[{
             SubscriptBox["\[CurlyEpsilon]", "0"], "/", 
             SuperscriptBox["c", "2"]}]}], ")"}], 
          "\[DoubleStruckCapitalV]"}]}], ")"}], 
       RowBox[{
        SubscriptBox["f", "0"], "(", 
        SubscriptBox["\[DoubleStruckP]", "0"], ")"}], 
       RowBox[{"\[DifferentialD]", 
        SubscriptBox["\[DoubleStruckP]", "0"]}]}]}]}]}], 
   TraditionalForm], "\[IndentingNewLine]", 
  FormBox[
   RowBox[{"=", 
    RowBox[{"\[Gamma]", 
     RowBox[{"\[Integral]", 
      RowBox[{
       RowBox[{"(", 
        RowBox[{
         SubscriptBox["\[DoubleStruckP]", "0"], "+", 
         RowBox[{
          RowBox[{"(", 
           RowBox[{"\[Gamma]", " ", 
            RowBox[{
             SubscriptBox["\[CurlyEpsilon]", "0"], "/", 
             SuperscriptBox["c", "2"]}]}], ")"}], "\[DoubleStruckCapitalV]"}],
          "+", 
         RowBox[{
          RowBox[{"(", 
           RowBox[{"\[Gamma]", "-", "1"}], ")"}], 
          RowBox[{
           RowBox[{"(", 
            RowBox[{"\[DoubleStruckCapitalV]", "\[CenterDot]", 
             SubscriptBox["\[DoubleStruckP]", "0"]}], ")"}], "/", 
           SuperscriptBox["V", "2"]}], "\[DoubleStruckCapitalV]"}], "+", 
         RowBox[{
          RowBox[{
           RowBox[{"(", 
            RowBox[{"\[DoubleStruckCapitalV]", "\[CenterDot]", 
             SubscriptBox["\[DoubleStruckP]", "0"]}], ")"}], "/", 
           SubscriptBox["\[CurlyEpsilon]", "0"]}], 
          SubscriptBox["\[DoubleStruckP]", "0"]}], "+", 
         RowBox[{
          RowBox[{"(", 
           RowBox[{"\[Gamma]", "-", "1"}], ")"}], 
          RowBox[{
           RowBox[{
            RowBox[{"(", 
             RowBox[{"\[DoubleStruckCapitalV]", "\[CenterDot]", 
              SubscriptBox["\[DoubleStruckP]", "0"]}], ")"}], "/", 
            RowBox[{
             SubscriptBox["\[CurlyEpsilon]", "0"], "(", 
             RowBox[{"\[DoubleStruckCapitalV]", "\[CenterDot]", 
              SubscriptBox["\[DoubleStruckP]", "0"]}], ")"}]}], "/", 
           SuperscriptBox["V", "2"]}], "\[DoubleStruckCapitalV]"}], "+", 
         RowBox[{
          RowBox[{
           RowBox[{"(", 
            RowBox[{"\[DoubleStruckCapitalV]", "\[CenterDot]", 
             SubscriptBox["\[DoubleStruckP]", "0"]}], ")"}], "/", 
           RowBox[{
            SubscriptBox["\[CurlyEpsilon]", "0"], "(", 
            RowBox[{"\[Gamma]", " ", 
             RowBox[{
              SubscriptBox["\[CurlyEpsilon]", "0"], "/", 
              SuperscriptBox["c", "2"]}]}], ")"}]}], 
          "\[DoubleStruckCapitalV]"}]}], ")"}], 
       RowBox[{
        SubscriptBox["f", "0"], "(", 
        SubscriptBox["\[DoubleStruckP]", "0"], ")"}], 
       RowBox[{"\[DifferentialD]", 
        SubscriptBox["\[DoubleStruckP]", "0"]}]}]}]}]}], 
   TraditionalForm], "\[IndentingNewLine]", 
  FormBox[
   RowBox[{"=", 
    RowBox[{"\[Gamma]", 
     RowBox[{"\[Integral]", 
      RowBox[{
       RowBox[{"(", 
        RowBox[{
         RowBox[{
          RowBox[{"(", 
           RowBox[{"\[Gamma]", " ", 
            RowBox[{
             SubscriptBox["\[CurlyEpsilon]", "0"], "/", 
             SuperscriptBox["c", "2"]}]}], ")"}], "\[DoubleStruckCapitalV]"}],
          "+", 
         RowBox[{
          RowBox[{
           RowBox[{"(", 
            RowBox[{"\[DoubleStruckCapitalV]", "\[CenterDot]", 
             SubscriptBox["\[DoubleStruckP]", "0"]}], ")"}], "/", 
           SubscriptBox["\[CurlyEpsilon]", "0"]}], 
          SubscriptBox["\[DoubleStruckP]", "0"]}], "+", 
         RowBox[{
          RowBox[{"(", 
           RowBox[{"\[Gamma]", "-", "1"}], ")"}], 
          RowBox[{
           RowBox[{
            SuperscriptBox[
             RowBox[{"(", 
              RowBox[{"\[DoubleStruckCapitalV]", "\[CenterDot]", 
